  :containsRhetoricalDevice :Symbol_Trispondoi .

:Line_432
  a owl:NamedIndividual, :Line ;
  :lineNumber 432 ;
  :canonicalReference "Episode 2.432"^^xsd:string ;
  :text "Χἠμεῖς ἰδόντες ἱέμεσθα, σὺν δέ νιν"^^xsd:string ;
  :hasTheme :Theme_Hunting, :Theme_Pursuit, :Theme_Tranquility .

:Line_433
  a owl:NamedIndividual, :Line ;
  :lineNumber 433 ;
  :canonicalReference "Episode 2.433"^^xsd:string ;
  :text "θηρώμεθ ̓ εὐθὺς οὐδὲν ἐκπεπληγμένην,"^^xsd:string .

:Line_434
  a owl:NamedIndividual, :Line ;
  :lineNumber 434 ;
  :canonicalReference "Episode 2.434"^^xsd:string ;
  :text "καὶ τάς τε πρόσθεν τάς τε νῦν ἠλέγχομεν"^^xsd:string .

:Line_435
  a owl:NamedIndividual, :Line ;
  :lineNumber 435 ;
  :canonicalReference "Episode 2.435"^^xsd:string ;
  :text "πράξεις· ἄπαρνος δ ̓ οὐδενὸς καθίστατο,"^^xsd:string .

:Line_436
  a owl:NamedIndividual, :Line ;
  :lineNumber 436 ;
  :canonicalReference "Episode 2.436"^^xsd:string ;
  :text "ἅμ ̓ ἡδέως ἔμοιγε κἀλγεινῶς ἅμα."^^xsd:string ;
  :hasTheme :Theme_AmbivalentEmotion, :Theme_Relief ;
  :containsRhetoricalDevice :Oxymoron_BittersweetJoy .

:Line_437
  a owl:NamedIndividual, :Line ;
  :lineNumber 437 ;
  :canonicalReference "Episode 2.437"^^xsd:string ;
  :text "Τὸ μὲν γὰρ αὐτὸν ἐκ κακῶν πεφευγέναι"^^xsd:string .

:Line_438
  a owl:NamedIndividual, :Line ;
  :lineNumber 438 ;
  :canonicalReference "Episode 2.438"^^xsd:string ;
  :text "ἥδιστον, ἐς κακὸν δὲ τοὺς φίλους ἄγειν"^^xsd:string .

:Line_439
  a owl:NamedIndividual, :Line ;
  :lineNumber 439 ;
  :canonicalReference "Episode 2.439"^^xsd:string ;
  :text "ἀλγεινόν· ἀλλὰ πάντα ταῦθ ̓ ἥσσω λαβεῖν"^^xsd:string .

:Line_440
  a owl:NamedIndividual, :Line ;
  :lineNumber 440 ;
  :canonicalReference "Episode 2.440"^^xsd:string ;
  :text "ἐμοὶ πέφυκεν τῆς ἐμῆς σωτηρίας."^^xsd:string ;
  :hasTheme :Theme_SelfPreservation, :Theme_Selfishness ;
  :hasMotivation :Motivation_SelfPreservation .

:Line_441
  a owl:NamedIndividual, :Line ;
  :lineNumber 441 ;
  :canonicalReference "Episode 2.441"^^xsd:string ;
  :text "Σὲ δή, σὲ τὴν νεύουσαν εἰς πέδον κάρα,"^^xsd:string ;
  :hasTheme :Theme_Silence, :Theme_DramaticTechnique .

:Line_442
  a owl:NamedIndividual, :Line ;
  :lineNumber 442 ;
  :canonicalReference "Episode 2.442"^^xsd:string ;
  :text "φῄς, ἢ καταρνῇ μὴ δεδρακέναι τάδε;"^^xsd:string .

:Line_443
  a owl:NamedIndividual, :Line ;
  :lineNumber 443 ;
  :canonicalReference "Episode 2.443"^^xsd:string ;
  :text "Καὶ φημὶ δρᾶσαι κοὐκ ἀπαρνοῦμαι τὸ μή."^^xsd:string ;
  :hasTheme :Theme_MoralCourage, :Theme_Defiance .

:Line_444
  a owl:NamedIndividual, :Line ;
  :lineNumber 444 ;
  :canonicalReference "Episode 2.444"^^xsd:string ;
  :text "Σὺ μὲν κομίζοις ἂν σεαυτὸν ᾗ θέλεις"^^xsd:string .

:Line_445
  a owl:NamedIndividual, :Line ;
  :lineNumber 445 ;
  :canonicalReference "Episode 2.445"^^xsd:string ;
  :text "ἔξω βαρείας αἰτίας ἐλεύθερον·"^^xsd:string .

:Line_446
  a owl:NamedIndividual, :Line ;
  :lineNumber 446 ;
  :canonicalReference "Episode 2.446"^^xsd:string ;
  :text "σὺ δ ̓ εἰπέ μοι μὴ μῆκος, ἀλλὰ συντόμως,"^^xsd:string .

:Line_447
  a owl:NamedIndividual, :Line ;
  :lineNumber 447 ;
  :canonicalReference "Episode 2.447"^^xsd:string ;
  :text "ᾔδησθα κηρυχθέντα μὴ πράσσειν τάδε;"^^xsd:string .

:Line_448
  a owl:NamedIndividual, :Line ;
  :lineNumber 448 ;
  :canonicalReference "Episode 2.448"^^xsd:string ;
  :text "Ἤιδη· τί δ ̓ οὐκ ἔμελλον; ἐμφανῆ γὰρ ἦν."^^xsd:string ;
  :hasTheme :Theme_Awareness, :Theme_ConsciousAction .

:Line_449
  a owl:NamedIndividual, :Line ;
  :lineNumber 449 ;
  :canonicalReference "Episode 2.449"^^xsd:string ;
  :text "Καὶ δῆτ ̓ ἐτόλμας τούσδ ̓ ὑπερβαίνειν νόμους;"^^xsd:string ;
  :hasConflict :Conflict_DivineVsHumanLaw ;
  :hasTheme :Theme_Authority .

:Line_450
  a owl:NamedIndividual, :Line ;
  :lineNumber 450 ;
  :canonicalReference "Episode 2.450"^^xsd:string ;
  :text "Οὐ γάρ τί μοι Ζεὺς ἦν ὁ κηρύξας τάδε,"^^xsd:string ;
  :referencesMyth :MythologicalBackground_Zeus ;
  :hasTheme :Theme_DivineLaw .

:Line_451
  a owl:NamedIndividual, :Line ;
  :lineNumber 451 ;
  :canonicalReference "Episode 2.451"^^xsd:string ;
  :text "οὐδ ̓ ἡ ξύνοικος τῶν κάτω θεῶν Δίκη"^^xsd:string ;
  :referencesMyth :MythologicalBackground_Dike, :MythologicalBackground_UnderworldGods ;
  :hasTheme :Theme_DivineLaw, :Theme_Justice .

:Line_452
  a owl:NamedIndividual, :Line ;
  :lineNumber 452 ;
  :canonicalReference "Episode 2.452"^^xsd:string ;
  :text "τοιούσδ ̓ ἐν ἀνθρώποισιν ὥρισεν νόμους,"^^xsd:string .

:Line_453
  a owl:NamedIndividual, :Line ;
  :lineNumber 453 ;
  :canonicalReference "Episode 2.453"^^xsd:string ;
  :text "οὐδὲ σθένειν τοσοῦτον ᾠόμην τὰ σὰ"^^xsd:string .

:Line_454
  a owl:NamedIndividual, :Line ;
  :lineNumber 454 ;
  :canonicalReference "Episode 2.454"^^xsd:string ;
  :text "κηρύγμαθ ̓ ὥστ ̓ ἄγραπτα κἀσφαλῆ θεῶν"^^xsd:string .

:Line_455
  a owl:NamedIndividual, :Line ;
  :lineNumber 455 ;
  :canonicalReference "Episode 2.455"^^xsd:string ;
  :text "νόμιμα δύνασθαι θνητὸν ὄνθ ̓ ὑπερδραμεῖν."^^xsd:string ;
  :hasTheme :Theme_DivineLaw, :Theme_MortalLimitation ;
  :containsRhetoricalDevice :Metaphor_UnwrittenLaws .

:Line_456
  a owl:NamedIndividual, :Line ;
  :lineNumber 456 ;
  :canonicalReference "Episode 2.456"^^xsd:string ;
  :text "Οὐ γάρ τι νῦν γε κἀχθές, ἀλλ ̓ ἀεί ποτε"^^xsd:string .

:Line_457
  a owl:NamedIndividual, :Line ;
  :lineNumber 457 ;
  :canonicalReference "Episode 2.457"^^xsd:string ;
  :text "ζῇ ταῦτα, κοὐδεὶς οἶδεν ἐξ ὅτου  ̓φάνη."^^xsd:string .

:Line_458
  a owl:NamedIndividual, :Line ;
  :lineNumber 458 ;
  :canonicalReference "Episode 2.458"^^xsd:string ;
  :text "Τούτων ἐγὼ οὐκ ἔμελλον, ἀνδρὸς οὐδενὸς"^^xsd:string .

:Line_459
  a owl:NamedIndividual, :Line ;
  :lineNumber 459 ;
  :canonicalReference "Episode 2.459"^^xsd:string ;
  :text "φρόνημα δείσασ ̓, ἐν θεοῖσι τὴν δίκην"^^xsd:string .

:Line_460
  a owl:NamedIndividual, :Line ;
  :lineNumber 460 ;
  :canonicalReference "Episode 2.460"^^xsd:string ;
  :text "δώσειν· θανουμένη γὰρ ἐξῄδη, τί δ ̓ οὔ;"^^xsd:string .

:Line_461
  a owl:NamedIndividual, :Line ;
  :lineNumber 461 ;
  :canonicalReference "Episode 2.461"^^xsd:string ;
  :text "κεἰ μὴ σὺ προὐκήρυξας. Εἰ δὲ τοῦ χρόνου"^^xsd:string .

:Line_462
  a owl:NamedIndividual, :Line ;
  :lineNumber 462 ;
  :canonicalReference "Episode 2.462"^^xsd:string ;
  :text "πρόσθεν θανοῦμαι, κέρδος αὔτ ̓ ἐγὼ λέγω·"^^xsd:string .

:Line_463
  a owl:NamedIndividual, :Line ;
  :lineNumber 463 ;
  :canonicalReference "Episode 2.463"^^xsd:string ;
  :text "ὅστις γὰρ ἐν πολλοῖσιν ὡς ἐγὼ κακοῖς"^^xsd:string .

:Line_464
  a owl:NamedIndividual, :Line ;
  :lineNumber 464 ;
  :canonicalReference "Episode 2.464"^^xsd:string ;
  :text "ζῇ, πῶς ὅδ ̓ οὐχὶ κατθανὼν κέρδος φέρει;"^^xsd:string .

:Line_465
  a owl:NamedIndividual, :Line ;
  :lineNumber 465 ;
  :canonicalReference "Episode 2.465"^^xsd:string ;
  :text "Οὕτως ἔμοιγε τοῦδε τοῦ μόρου τυχεῖν"^^xsd:string .

:Line_466
  a owl:NamedIndividual, :Line ;
  :lineNumber 466 ;
  :canonicalReference "Episode 2.466"^^xsd:string ;
  :text "παρ ̓ οὐδὲν ἄλγος· ἀλλ ̓ ἄν, εἰ τὸν ἐξ ἐμῆς"^^xsd:string .

:Line_467
  a owl:NamedIndividual, :Line ;
  :lineNumber 467 ;
  :canonicalReference "Episode 2.467"^^xsd:string ;
  :text μητρὸς θανόντ ̓ ἄθαπτον ἠνσχόμην νέκυν,"^^xsd:string ;
  :hasTheme :Theme_FamilyDuty, :Theme_BurialRites .

:Line_468
  a owl:NamedIndividual, :Line ;
  :lineNumber 468 ;
  :canonicalReference "Episode 2.468"^^xsd:string ;
  :text "κείνοις ἂν ἤλγουν· τοῖσδε δ ̓ οὐκ ἀλγύνομαι."^^xsd:string .

:Line_469
  a owl:NamedIndividual, :Line ;
  :lineNumber 469 ;
  :canonicalReference "Episode 2.469"^^xsd:string ;
  :text "Σοὶ δ ̓ εἰ δοκῶ νῦν μῶρα δρῶσα τυγχάνειν,"^^xsd:string .

:Line_470
  a owl:NamedIndividual, :Line ;
  :lineNumber 470 ;
  :canonicalReference "Episode 2.470"^^xsd:string ;
  :text "σχεδόν τι μώρῳ μωρίαν ὀφλισκάνω."^^xsd:string .

:Line_471
  a owl:NamedIndividual, :Line ;
  :lineNumber 471 ;
  :canonicalReference "Episode 2.471"^^xsd:string ;
  :text "Δηλοῖ τὸ γέννημ ̓ ὠμὸν ἐξ ὠμοῦ πατρὸς"^^xsd:string ;
  :hasTheme :Theme_FamilyHeritage, :Theme_CharacterTrait ;
  :referencesMyth :MythologicalBackground_Oedipus .

:Line_472
  a owl:NamedIndividual, :Line ;
  :lineNumber 472 ;
  :canonicalReference "Episode 2.472"^^xsd:string ;
  :text "τῆς παιδός· εἴκειν δ ̓ οὐκ ἐπίσταται κακοῖς."^^xsd:string .

:Line_473
  a owl:NamedIndividual, :Line ;
  :lineNumber 473 ;
  :canonicalReference "Episode 2.473"^^xsd:string ;
  :text "Ἀλλ ̓ ἴσθι τοι τὰ σκλήρ ̓ ἄγαν φρονήματα"^^xsd:string .

:Line_474
  a owl:NamedIndividual, :Line ;
  :lineNumber 474 ;
  :canonicalReference "Episode 2.474"^^xsd:string ;
  :text "πίπτειν μάλιστα, καὶ τὸν ἐγκρατέστατον"^^xsd:string ;
  :hasTheme :Theme_CharacterTrait, :Theme_TragicFlaw ;
  :containsRhetoricalDevice :Metaphor_Iron .

:Line_475
  a owl:NamedIndividual, :Line ;
  :lineNumber 475 ;
  :canonicalReference "Episode 2.475"^^xsd:string ;
  :text "σίδηρον ὀπτὸν ἐκ πυρὸς περισκελῆ"^^xsd:string .

:Line_476
  a owl:NamedIndividual, :Line ;
  :lineNumber 476 ;
  :canonicalReference "Episode 2.476"^^xsd:string ;
  :text "θραυσθέντα καὶ ῥαγέντα πλεῖστ ̓ ἂν εἰσίδοις."^^xsd:string .

:Line_477
  a owl:NamedIndividual, :Line ;
  :lineNumber 477 ;
  :canonicalReference "Episode 2.477"^^xsd:string ;
  :text "Σμικρῷ χαλινῷ δ ̓ οἶδα τοὺς θυμουμένους"^^xsd:string .

:Line_478
  a owl:NamedIndividual, :Line ;
  :lineNumber 478 ;
  :canonicalReference "Episode 2.478"^^xsd:string ;
  :text "ἵππους καταρτυθέντας· οὐ γὰρ ἐκπέλει"^^xsd:string .

:Line_479
  a owl:NamedIndividual, :Line ;
  :lineNumber 479 ;
  :canonicalReference "Episode 2.479"^^xsd:string ;
  :text "φρονεῖν μέγ ̓ ὅστις δοῦλός ἐστι τῶν πέλας."^^xsd:string .

:Line_480
  a owl:NamedIndividual, :Line ;
  :lineNumber 480 ;
  :canonicalReference "Episode 2.480"^^xsd:string ;
  :text "Αὕτη δ ̓ ὑβρίζειν μὲν τότ ̓ ἐξηπίστατο,"^^xsd:string .

:Line_481
  a owl:NamedIndividual, :Line ;
  :lineNumber 481 ;
  :canonicalReference "Episode 2.481"^^xsd:string ;
  :text "νόμους ὑπερβαίνουσα τοὺς προκειμένους·"^^xsd:string .

:Line_482
  a owl:NamedIndividual, :Line ;
  :lineNumber 482 ;
  :canonicalReference "Episode 2.482"^^xsd:string ;
  :text "ὕβρις δ ̓, ἐπεὶ δέδρακεν, ἥδε δευτέρα,"^^xsd:string .

:Line_483
  a owl:NamedIndividual, :Line ;
  :lineNumber 483 ;
  :canonicalReference "Episode 2.483"^^xsd:string ;
  :text "τούτοις ἐπαυχεῖν καὶ δεδρακυῖαν γελᾶν."^^xsd:string .

:Line_484
  a owl:NamedIndividual, :Line ;
  :lineNumber 484 ;
  :canonicalReference "Episode 2.484"^^xsd:string ;
  :text "Ἦ νῦν ἐγὼ μὲν οὐκ ἀνήρ, αὕτη δ ̓ ἀνήρ,"^^xsd:string ;
  :hasTheme :Theme_GenderRoles ;
  :containsRhetoricalDevice :Irony_GenderReversal .

:Line_485
  a owl:NamedIndividual, :Line ;
  :lineNumber 485 ;
  :canonicalReference "Episode 2.485"^^xsd:string ;
  :text εἰ ταῦτ ̓ ἀνατὶ τῇδε κείσεται κράτη."^^xsd:string .

:Line_486
  a owl:NamedIndividual, :Line ;
  :lineNumber 486 ;
  :canonicalReference "Episode 2.486"^^xsd:string ;
  :text "Ἀλλ ̓ εἴτ ̓ ἀδελφῆς εἴθ ̓ ὁμαιμονεστέρα"^^xsd:string .

:Line_487
  a owl:NamedIndividual, :Line ;
  :lineNumber 487 ;
  :canonicalReference "Episode 2.487"^^xsd:string ;
  :text "τοῦ παντὸς ἡμῖν Ζηνὸς Ἑρκείου κυρεῖ,"^^xsd:string ;
  :referencesMyth :MythologicalBackground_ZeusHerkeios ;
  :hasTheme :Theme_FamilyBond, :Theme_DivineLaw .

:Line_488
  a owl:NamedIndividual, :Line ;
  :lineNumber 488 ;
  :canonicalReference "Episode 2.488"^^xsd:string ;
  :text "αὐτή τε χἠ ξύναιμος οὐκ ἀλύξετον"^^xsd:string .

:Line_489
  a owl:NamedIndividual, :Line ;
  :lineNumber 489 ;
  :canonicalReference "Episode 2.489"^^xsd:string ;
  :text "μόρου κακίστου· καὶ γὰρ οὖν κείνην ἴσον"^^xsd:string .

:Line_490
  a owl:NamedIndividual, :Line ;
  :lineNumber 490 ;
  :canonicalReference "Episode 2.490"^^xsd:string ;
  :text "ἐπαιτιῶμαι τοῦδε βουλεῦσαι τάφου."^^xsd:string .

:Line_491
  a owl:NamedIndividual, :Line ;
  :lineNumber 491 ;
  :canonicalReference "Episode 2.491"^^xsd:string ;
  :text "Καί νιν καλεῖτ ̓· ἔσω γὰρ εἶδον ἀρτίως"^^xsd:string .

:Line_492
  a owl:NamedIndividual, :Line ;
  :lineNumber 492 ;
  :canonicalReference "Episode 2.492"^^xsd:string ;
  :text "λυσσῶσαν αὐτὴν οὐδ ̓ ἐπήβολον φρενῶν."^^xsd:string .

:Line_493
  a owl:NamedIndividual, :Line ;
  :lineNumber 493 ;
  :canonicalReference "Episode 2.493"^^xsd:string ;
  :text "Φιλεῖ δ ̓ ὁ θυμὸς πρόσθεν ᾑρῆσθαι κλοπεὺς"^^xsd:string .

:Line_494
  a owl:NamedIndividual, :Line ;
  :lineNumber 494 ;
  :canonicalReference "Episode 2.494"^^xsd:string ;
  :text "τῶν μηδὲν ὀρθῶς ἐν σκότῳ τεχνωμένων."^^xsd:string .

:Line_495
  a owl:NamedIndividual, :Line ;
  :lineNumber 495 ;
  :canonicalReference "Episode 2.495"^^xsd:string ;
  :text "Μισῶ γε μέντοι χὤταν ἐν κακοῖσί τις"^^xsd:string .

:Line_496
  a owl:NamedIndividual, :Line ;
  :lineNumber 496 ;
  :canonicalReference "Episode 2.496"^^xsd:string ;
  :text "ἁλοὺς ἔπειτα τοῦτο καλλύνειν θέλῃ."^^xsd:string .

:Line_497
  a owl:NamedIndividual, :Line ;
  :lineNumber 497 ;
  :canonicalReference "Episode 2.497"^^xsd:string ;
  :text "Θέλεις τι μεῖζον ἢ κατακτεῖναί μ ̓ ἑλών;"^^xsd:string ;
  :hasTheme :Theme_Death, :Theme_Acceptance .

:Line_498
  a owl:NamedIndividual, :Line ;
  :lineNumber 498 ;
  :canonicalReference "Episode 2.498"^^xsd:string ;
  :text "Ἐγὼ μὲν οὐδέν· τοῦτ ̓ ἔχων ἅπαντ ̓ ἔχω."^^xsd:string ;
  :hasTheme :Theme_Justice, :Theme_Authority .

:Line_499
  a owl:NamedIndividual, :Line ;
  :lineNumber 499 ;
  :canonicalReference "Episode 2.499"^^xsd:string ;
  :text "Τί δῆτα μέλλεις; ὡς ἐμοὶ τῶν σῶν λόγων"^^xsd:string .

:Line_500
  a owl:NamedIndividual, :Line ;
  :lineNumber 500 ;
  :canonicalReference "Episode 2.500"^^xsd:string ;
  :text "ἀρεστὸν οὐδέν, μηδ ̓ ἀρεσθείη ποτέ,"^^xsd:string .

:Line_501
  a owl:NamedIndividual, :Line ;
  :lineNumber 501 ;
  :canonicalReference "Episode 2.501"^^xsd:string ;
  :text "οὕτω δὲ καὶ σοὶ τἄμ ̓ ἀφανδάνοντ ̓ ἔφυ."^^xsd:string .

:Line_502
  a owl:NamedIndividual, :Line ;
  :lineNumber 502 ;
  :canonicalReference "Episode 2.502"^^xsd:string ;
  :text "Καίτοι πόθεν κλέος γ ̓ ἂν εὐκλεέστερον"^^xsd:string .

:Line_503
  a owl:NamedIndividual, :Line ;
  :lineNumber 503 ;
  :canonicalReference "Episode 2.503"^^xsd:string ;
  :text "κατέσχον ἢ τὸν αὐτάδελφον ἐν τάφῳ"^^xsd:string .

:Line_504
  a owl:NamedIndividual, :Line ;
  :lineNumber 504 ;
  :canonicalReference "Episode 2.504"^^xsd:string ;
  :text "τιθεῖσα; τούτοις τοῦτο πᾶσιν ἁνδάνειν"^^xsd:string ;
  :hasTheme :Theme_PublicOpinion, :Theme_FearOfTyrant .

:Line_505
  a owl:NamedIndividual, :Line ;
  :lineNumber 505 ;
  :canonicalReference "Episode 2.505"^^xsd:string ;
  :text "λέγοιτ ̓ ἄν, εἰ μὴ γλῶσσαν ἐγκλῄοι φόβος."^^xsd:string ;
  :hasTheme :Theme_FearOfTyrant, :Theme_PublicOpinion .

:Line_506
  a owl:NamedIndividual, :Line ;
  :lineNumber 506 ;
  :canonicalReference "Episode 2.506"^^xsd:string ;
  :text "Ἀλλ ̓ ἡ τυραννὶς πολλά τ ̓ ἄλλ ̓ εὐδαιμονεῖ"^^xsd:string ;
  :hasTheme :Theme_Tyranny, :Theme_Authority .

:Line_507
  a owl:NamedIndividual, :Line ;
  :lineNumber 507 ;
  :canonicalReference "Episode 2.507"^^xsd:string ;
  :text "κἄξεστιν αὐτῇ δρᾶν λέγειν θ ̓ ἃ βούλεται."^^xsd:string ;
  :hasTheme :Theme_Tyranny, :Theme_Authority .

:Line_508
  a owl:NamedIndividual, :Line ;
  :lineNumber 508 ;
  :canonicalReference "Episode 2.508"^^xsd:string ;
  :text "Σὺ τοῦτο μούνη τῶνδε Καδμείων ὁρᾷς."^^xsd:string ;
  :hasTheme :Theme_Tyranny, :Theme_Disagreement, :Theme_Authority ;
  :referencesMyth :MythologicalBackground_Cadmus .

:Line_509
  a owl:NamedIndividual, :Line ;
  :lineNumber 509 ;
  :canonicalReference "Episode 2.509"^^xsd:string ;
  :text "Ὁρῶσι χοὖτοι· σοὶ δ ̓ ὑπίλλουσι στόμα."^^xsd:string ;
  :hasTheme :Theme_FearOfTyrant, :Theme_PublicOpinion ;
  :referencesMyth :MythologicalBackground_Cadmus .

:Line_510
  a owl:NamedIndividual, :Line ;
  :lineNumber 510 ;
  :canonicalReference "Episode 2.510"^^xsd:string ;
  :text "Σὺ δ ̓ οὐκ ἐπαιδῇ, τῶνδε χωρὶς εἰ φρονεῖς;"^^xsd:string ;
  :hasTheme :Theme_Accusation, :Theme_Authority, :Theme_Contempt .

:Line_511
  a owl:NamedIndividual, :Line ;
  :lineNumber 511 ;
  :canonicalReference "Episode 2.511"^^xsd:string ;
  :text "Οὐδὲν γὰρ αἰσχρὸν τοὺς ὁμοσπλάγχνους σέβειν."^^xsd:string ;
  :hasTheme :Theme_DivineLaw, :Theme_FamilyDuty, :Theme_FamilyBond .

:Line_512
  a owl:NamedIndividual, :Line ;
  :lineNumber 512 ;
  :canonicalReference "Episode 2.512"^^xsd:string ;
  :text "Οὔκουν ὅμαιμος χὠ καταντίον θανών;"^^xsd:string ;
  :hasTheme :Theme_FamilyBond, :Theme_Disagreement .

:Line_513
  a owl:NamedIndividual, :Line ;
  :lineNumber 513 ;
  :canonicalReference "Episode 2.513"^^xsd:string ;
  :text "Ὅμαιμος ἐκ μιᾶς τε καὶ ταὐτοῦ πατρός."^^xsd:string ;
  :hasTheme :Theme_FamilyBond .

:Line_514
  a owl:NamedIndividual, :Line ;
  :lineNumber 514 ;
  :canonicalReference "Episode 2.514"^^xsd:string ;
  :text "Πῶς δῆτ ̓ ἐκείνῳ δυσσεβῆ τιμᾷς χάριν;"^^xsd:string ;
  :hasTheme :Theme_Accusation, :Theme_Disagreement, :Theme_FamilyDuty .

:Line_515
  a owl:NamedIndividual, :Line ;
  :lineNumber 515 ;
  :canonicalReference "Episode 2.515"^^xsd:string ;
  :text "Οὐ μαρτυρήσει ταῦθ ̓ ὁ κατθανὼν νέκυς."^^xsd:string ;
  :hasTheme :Theme_Death, :Theme_Justice, :Theme_Disagreement .

:Line_516
  a owl:NamedIndividual, :Line ;
  :lineNumber 516 ;
  :canonicalReference "Episode 2.516"^^xsd:string ;
  :text "Εἴ τοί σφε τιμᾷς ἐξ ἴσου τῷ δυσσεβεῖ."^^xsd:string ;
  :hasTheme :Theme_Accusation, :Theme_Disagreement, :Theme_Authority .

:Line_517
  a owl:NamedIndividual, :Line ;
  :lineNumber 517 ;
  :canonicalReference "Episode 2.517"^^xsd:string ;
  :text "Οὐ γάρ τι δοῦλος, ἀλλ ̓ ἀδελφὸς ὤλετο."^^xsd:string ;
  :hasTheme :Theme_FamilyBond, :Theme_FamilyDuty, :Theme_Defiance .

:Line_518
  a owl:NamedIndividual, :Line ;
  :lineNumber 518 ;
  :canonicalReference "Episode 2.518"^^xsd:string ;
  :text "Πορθῶν δὲ τήνδε γῆν· ὁ δ ̓ ἀντιστὰς ὕπερ."^^xsd:string ;
  :hasTheme :Theme_CharacterTrait, :Theme_Authority, :Theme_Disagreement .

:Line_519
  a owl:NamedIndividual, :Line ;
  :lineNumber 519 ;
  :canonicalReference "Episode 2.519"^^xsd:string ;
  :text "Ὅμως ὅ γ ̓ Ἅιδης τοὺς νόμους ἴσους ποθεῖ."^^xsd:string ;
  :referencesMyth :MythologicalBackground_Hades ;
  :hasTheme :Theme_Justice, :Theme_Equality .

:Line_520
  a owl:NamedIndividual, :Line ;
  :lineNumber 520 ;
  :canonicalReference "Episode 2.520"^^xsd:string ;
  :text "Ἀλλ ̓ οὐχ ὁ χρηστὸς τῷ κακῷ λαχεῖν ἴσος."^^xsd:string ;
  :hasTheme :Theme_Disagreement, :Theme_Authority, :Theme_CharacterTrait .

:Line_521
  a owl:NamedIndividual, :Line ;
  :lineNumber 521 ;
  :canonicalReference "Episode 2.521"^^xsd:string ;
  :text "Τίς οἶδεν εἰ κάτωθεν εὐαγῆ τάδε;"^^xsd:string ;
  :hasTheme :Theme_MortalLimitation, :Theme_Disagreement .

:Line_522
  a owl:NamedIndividual, :Line ;
  :lineNumber 522 ;
  :canonicalReference "Episode 2.522"^^xsd:string ;
  :text "Οὔτοι ποθ ̓ οὑχθρός, οὐδ ̓ ὅταν θάνῃ, φίλος."^^xsd:string ;
  :hasTheme :Theme_Disagreement, :Theme_Authority, :Theme_Death .

:Line_523
  a owl:NamedIndividual, :Line ;
  :lineNumber 523 ;
  :canonicalReference "Episode 2.523"^^xsd:string ;
  :text "Οὔτοι συνέχθειν, ἀλλὰ συμφιλεῖν ἔφυν."^^xsd:string ;
  :hasTheme :Theme_Sisterhood, :Theme_Love, :Theme_EmotionalState ;
  :hasMotivation :Motivation_ProtectSister .

:Line_524
  a owl:NamedIndividual, :Line ;
  :lineNumber 524 ;
  :canonicalReference "Episode 2.524"^^xsd:string ;
  :text "Κάτω νυν ἐλθοῦσ ̓, εἰ φιλητέον, φίλει"^^xsd:string ;
  :hasTheme :Theme_Refusal, :Theme_Love, :Theme_Death, :Theme_GenderRoles .

:Line_525
  a owl:NamedIndividual, :Line ;
  :lineNumber 525 ;
  :canonicalReference "Episode 2.525"^^xsd:string ;
  :text "κείνους· ἐμοῦ δὲ ζῶντος οὐκ ἄρξει γυνή."^^xsd:string ;
  :hasTheme :Theme_GenderRoles, :Theme_Authority, :Theme_Refusal .

:Line_526
  a owl:NamedIndividual, :Line ;
  :lineNumber 526 ;
  :canonicalReference "Episode 2.526"^^xsd:string ;
  :text "Καὶ μὴν πρὸ πυλῶν ἥδ ̓ Ἰσμήνη,"^^xsd:string ;
  :hasTheme :Theme_Entrance, :Theme_Announcement .

:Line_527
  a owl:NamedIndividual, :Line ;
  :lineNumber 527 ;
  :canonicalReference "Episode 2.527"^^xsd:string ;
  :text "φιλάδελφα κάτω δάκρυ ̓ εἰβομένη·"^^xsd:string ;
  :hasTheme :Theme_Sisterhood, :Theme_Sorrow, :Theme_EmotionalState .

:Line_528
  a owl:NamedIndividual, :Line ;
  :lineNumber 528 ;
  :canonicalReference "Episode 2.528"^^xsd:string ;
  :text "νεφέλη δ ̓ ὀφρύων ὕπερ αἱματόεν"^^xsd:string .

:Line_529
  a owl:NamedIndividual, :Line ;
  :lineNumber 529 ;
  :canonicalReference "Episode 2.529"^^xsd:string ;
  :text "ῥέθος αἰσχύνει,"^^xsd:string .

:Line_530
  a owl:NamedIndividual, :Line ;
  :lineNumber 530 ;
  :canonicalReference "Episode 2.530"^^xsd:string ;
  :text "τέγγουσ ̓ εὐῶπα παρειάν."^^xsd:string .

:Line_531
  a owl:NamedIndividual, :Line ;
  :lineNumber 531 ;
  :canonicalReference "Episode 2.531"^^xsd:string ;
  :text "Σὺ δ ̓, ἣ κατ ̓ οἴκους ὡς ἔχιδν ̓ ὑφειμένη"^^xsd:string ;
  :hasTheme :Theme_Accusation, :Theme_Insult, :Theme_Treachery ;
  :containsRhetoricalDevice :Metaphor_Serpent .

:Line_532
  a owl:NamedIndividual, :Line ;
  :lineNumber 532 ;
  :canonicalReference "Episode 2.532"^^xsd:string ;
  :text "λήθουσά μ ̓ ἐξέπινες, οὐδ ̓ ἐμάνθανον"^^xsd:string .

:Line_533
  a owl:NamedIndividual, :Line ;
  :lineNumber 533 ;
  :canonicalReference "Episode 2.533"^^xsd:string ;
  :text "τρέφων δύ ̓ ἄτα κἀπαναστάσεις θρόνων,"^^xsd:string ;
  :hasTheme :Theme_Accusation .

:Line_534
  a owl:NamedIndividual, :Line ;
  :lineNumber 534 ;
  :canonicalReference "Episode 2.534"^^xsd:string ;
  :text "φέρ ̓, εἰπὲ δή μοι, καὶ σὺ τοῦδε τοῦ τάφου"^^xsd:string .

:Line_535
  a owl:NamedIndividual, :Line ;
  :lineNumber 535 ;
  :canonicalReference "Episode 2.535"^^xsd:string ;
  :text "φήσεις μετασχεῖν, ἢ  ̓ξομῇ τὸ μὴ εἰδέναι;"^^xsd:string .

:Line_536
  a owl:NamedIndividual, :Line ;
  :lineNumber 536 ;
  :canonicalReference "Episode 2.536"^^xsd:string ;
  :text "Δέδρακα τοὔργον, εἴπερ ἥδ ̓ ὁμορροθεῖ,"^^xsd:string ;
  :hasTheme :Theme_Sisterhood, :Theme_Love ;
  :hasMotivation :Motivation_ShareFate .

:Line_537
  a owl:NamedIndividual, :Line ;
  :lineNumber 537 ;
  :canonicalReference "Episode 2.537"^^xsd:string ;
  :text "καὶ ξυμμετίσχω καὶ φέρω τῆς αἰτίας."^^xsd:string .

:Line_538
  a owl:NamedIndividual, :Line ;
  :lineNumber 538 ;
  :canonicalReference "Episode 2.538"^^xsd:string ;
  :text "Ἀλλ ̓ οὐκ ἐάσει τοῦτό γ ̓ ἡ δίκη σ ̓, ἐπεὶ"^^xsd:string ;
  :hasTheme :Theme_Justice, :Theme_DivineJustice, :Theme_Truth ;
  :referencesMyth :MythologicalBackground_Nemesis .

:Line_539
  a owl:NamedIndividual, :Line ;
  :lineNumber 539 ;
  :canonicalReference "Episode 2.539"^^xsd:string ;
  :text "οὔτ ̓ ἠθέλησας οὔτ ̓ ἐγὼ  ̓κοινωσάμην."^^xsd:string ;
  :hasTheme :Theme_Truth, :Theme_Justice, :Theme_Refusal .

:Line_540
  a owl:NamedIndividual, :Line ;
  :lineNumber 540 ;
  :canonicalReference "Episode 2.540"^^xsd:string ;
  :text "Ἀλλ ̓ ἐν κακοῖς τοῖς σοῖσιν οὐκ αἰσχύνομαι"^^xsd:string ;
  :hasTheme :Theme_Sisterhood, :Theme_Love, :Theme_SharedResponsibility .

:Line_541
  a owl:NamedIndividual, :Line ;
  :lineNumber 541 ;
  :canonicalReference "Episode 2.541"^^xsd:string ;
  :text "ξύμπλουν ἐμαυτὴν τοῦ πάθους ποιουμένη."^^xsd:string ;
  :hasTheme :Theme_Sisterhood, :Theme_Love, :Theme_SharedResponsibility .

:Line_542
  a owl:NamedIndividual, :Line ;
  :lineNumber 542 ;
  :canonicalReference "Episode 2.542"^^xsd:string ;
  :text "Ὧν τοὔργον Ἅιδης χοἰ κάτω ξυνίστορες·"^^xsd:string ;
  :hasTheme :Theme_DivineJustice, :Theme_Refusal, :Theme_Truth ;
  :referencesMyth :MythologicalBackground_Hades .

:Line_543
  a owl:NamedIndividual, :Line ;
  :lineNumber 543 ;
  :canonicalReference "Episode 2.543"^^xsd:string ;
  :text "λόγοις δ ̓ ἐγὼ φιλοῦσαν οὐ στέργω φίλην."^^xsd:string ;
  :hasTheme :Theme_Refusal, :Theme_Truth, :Theme_Justice .

:Line_544
  a owl:NamedIndividual, :Line ;
  :lineNumber 544 ;
  :canonicalReference "Episode 2.544"^^xsd:string ;
  :text "Μήτοι, κασιγνήτη, μ ̓ ἀτιμάσῃς τὸ μὴ οὐ"^^xsd:string ;
  :hasTheme :Theme_Sisterhood, :Theme_Love, :Theme_Guilt .

:Line_545
  a owl:NamedIndividual, :Line ;
  :lineNumber 545 ;
  :canonicalReference "Episode 2.545"^^xsd:string ;
  :text "θανεῖν τε σὺν σοὶ τὸν θανόντα θ ̓ ἁγνίσαι."^^xsd:string ;
  :hasTheme :Theme_FamilyDuty, :Theme_Guilt, :Theme_HonorBrother .

:Line_546
  a owl:NamedIndividual, :Line ;
  :lineNumber 546 ;
  :canonicalReference "Episode 2.546"^^xsd:string ;
  :text "Μή μοι θάνῃς σὺ κοινά, μηδ ̓ ἃ μὴ  ̓θιγες"^^xsd:string ;
