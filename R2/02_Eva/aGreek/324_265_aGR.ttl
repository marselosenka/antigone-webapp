############################
# LINES (Ancient Greek MASTER)
############################

:Line_366 a owl:NamedIndividual, :Line ;
  :lineNumber 366 ; :canonicalReference "366"^^xsd:string ;
  :text "τέχνας ὑπὲρ ἐλπίδ ̓ ἔχων,"^^xsd:string .

:Line_367 a owl:NamedIndividual, :Line ;
  :lineNumber 367 ; :canonicalReference "367"^^xsd:string ;
  :text "τοτὲ μὲν κακόν, ἄλλοτ ̓ ἐπ ̓ ἐσθλὸν ἕρπει,"^^xsd:string .

:Line_368 a owl:NamedIndividual, :Line ;
  :lineNumber 368 ; :canonicalReference "368"^^xsd:string ;
  :text "νόμους περαίνων χθονὸς"^^xsd:string .

:Line_369 a owl:NamedIndividual, :Line ;
  :lineNumber 369 ; :canonicalReference "369"^^xsd:string ;
  :text "θεῶν τ ̓ ἔνορκον δίκαν·"^^xsd:string .

:Line_370 a owl:NamedIndividual, :Line ;
  :lineNumber 370 ; :canonicalReference "370"^^xsd:string ;
  :text "ὑψίπολις· ἄπολις ὅτῳ τὸ μὴ καλὸν"^^xsd:string .

:Line_371 a owl:NamedIndividual, :Line ;
  :lineNumber 371 ; :canonicalReference "371"^^xsd:string ;
  :text "ξύνεστι τόλμας χάριν·"^^xsd:string .

:Line_372 a owl:NamedIndividual, :Line ;
  :lineNumber 372 ; :canonicalReference "372"^^xsd:string ;
  :text "μήτ ̓ ἐμοὶ παρέστιος"^^xsd:string .

:Line_373 a owl:NamedIndividual, :Line ;
  :lineNumber 373 ; :canonicalReference "373"^^xsd:string ;
  :text "γένοιτο μήτ ̓ ἴσον φρονῶν"^^xsd:string .

:Line_374 a owl:NamedIndividual, :Line ;
  :lineNumber 374 ; :canonicalReference "374"^^xsd:string ;
  :text "ὃς τάδ ̓ ἔρδοι."^^xsd:string .

:Line_375 a owl:NamedIndividual, :Line ;
  :lineNumber 375 ; :canonicalReference "375"^^xsd:string ;
  :text "Ἐς δαιμόνιον τέρας ἀμφινοῶ"^^xsd:string .

:Line_376 a owl:NamedIndividual, :Line ;
  :lineNumber 376 ; :canonicalReference "376"^^xsd:string ;
  :text "τόδε· πῶς εἰδὼς ἀντιλογήσω"^^xsd:string .

:Line_377 a owl:NamedIndividual, :Line ;
  :lineNumber 377 ; :canonicalReference "377"^^xsd:string ;
  :text "τήνδ ̓ οὐκ εἶναι παῖδ ̓ Ἀντιγόνην;"^^xsd:string .

:Line_378 a owl:NamedIndividual, :Line ;
  :lineNumber 378 ; :canonicalReference "378"^^xsd:string ;
  :text "Ὦ δύστηνος καὶ δυστήνου"^^xsd:string .

:Line_379 a owl:NamedIndividual, :Line ;
  :lineNumber 379 ; :canonicalReference "379"^^xsd:string ;
  :text "πατρὸς Οἰδιπόδα,"^^xsd:string .

:Line_380 a owl:NamedIndividual, :Line ;
  :lineNumber 380 ; :canonicalReference "380"^^xsd:string ;
  :text "τί ποτ ̓; οὐ δή που σέ γ ̓ ἀπιστοῦσαν"^^xsd:string .

:Line_381 a owl:NamedIndividual, :Line ;
  :lineNumber 381 ; :canonicalReference "381"^^xsd:string ;
  :text "τοῖς βασιλείοισιν ἄγουσι νόμοις"^^xsd:string .

:Line_382 a owl:NamedIndividual, :Line ;
  :lineNumber 382 ; :canonicalReference "382"^^xsd:string ;
  :text "καὶ ἐν ἀφροσύνῃ καθελόντες;"^^xsd:string .

:Line_383 a owl:NamedIndividual, :Line ;
  :lineNumber 383 ; :canonicalReference "383"^^xsd:string ;
  :text "ΦΥ. Ἥδ ̓ ἔστ ̓ ἐκείνη τοὔργον ἡ  ̓ξειργασμένη·"^^xsd:string .

:Line_384 a owl:NamedIndividual, :Line ;
  :lineNumber 384 ; :canonicalReference "384"^^xsd:string ;
  :text "τήνδ ̓ εἵλομεν θάπτουσαν· ἀλλὰ ποῦ Κρέων;"^^xsd:string .

:Line_385 a owl:NamedIndividual, :Line ;
  :lineNumber 385 ; :canonicalReference "385"^^xsd:string ;
  :text "ΧΟ. Ὅδ ̓ ἐκ δόμων ἄψορρος εἰς δέον περᾷ."^^xsd:string .

:Line_386 a owl:NamedIndividual, :Line ;
  :lineNumber 386 ; :canonicalReference "386"^^xsd:string ;
  :text "ΚΡ. Τί δ ̓ ἔστι; ποίᾳ ξύμμετρος προὔβην τύχῃ;"^^xsd:string .

:Line_387 a owl:NamedIndividual, :Line ;
  :lineNumber 387 ; :canonicalReference "387"^^xsd:string ;
  :text ""^^xsd:string .

############################
# INTENTIONS AND OBJECTIVES
############################

:Motivation_Chorus_ExcludeLawless_370_374
  a owl:NamedIndividual, :Motivation ;
  :description "Refuse association (as hearth-mate or like-minded) with anyone who does these deeds and embraces the not-good through daring."^^xsd:string .

:Motivation_Guard_BringAntigone_383_384
  a owl:NamedIndividual, :Motivation ;
  :description "Present Antigone as the doer; report that she was caught burying."^^xsd:string .

:MoralDecision_Guard_DeliverCapturedDoer_383_384
  a owl:NamedIndividual, :MoralDecision ;
  :description "Deliver the captured burying agent to Creon for judgment."^^xsd:string .

:Motivation_Chorus_AssessIdentity_375_377
  a owl:NamedIndividual, :Motivation ;
  :description "Try to reconcile what is seen with knowledge of Antigone’s identity (astonished recognition)."^^xsd:string .

:Motivation_Creon_SeekExplanation_386
  a owl:NamedIndividual, :Motivation ;
  :description "Demand to know what has happened and what 'fortune' this situation matches."^^xsd:string .

:Antigone_Character_Chorus
  :hasMotivation :Motivation_Chorus_ExcludeLawless_370_374, :Motivation_Chorus_AssessIdentity_375_377 .

:Antigone_Character_Guard
  :hasMotivation :Motivation_Guard_BringAntigone_383_384 ;
  :makesMoralDecision :MoralDecision_Guard_DeliverCapturedDoer_383_384 .

:Antigone_Character_Creon
  :hasMotivation :Motivation_Creon_SeekExplanation_386 .

############################
# MORAL JUSTIFICATIONS
############################

:EthicalPrinciple_ObeyEarthlyLawsAndDivineJustice_368_369
  a owl:NamedIndividual, :EthicalPrinciple ;
  :description "Right civic standing depends on fulfilling the laws of the land and the gods’ sworn justice."^^xsd:string .

:EthicalPrinciple_CivicBelongingDependsOnTheGood_370_371
  a owl:NamedIndividual, :EthicalPrinciple ;
  :description "One is 'high in the city' if aligned with the good; 'without a city' if the not-good is embraced for daring."^^xsd:string .

:EthicalPrinciple_DisobedienceToRoyalLawsCondemned_380_382
  a owl:NamedIndividual, :EthicalPrinciple ;
  :description "The Chorus frames disobedience to royal laws (and folly) as blameworthy grounds for being led away."^^xsd:string .

:Antigone_Character_Chorus
  :advocatesFor :EthicalPrinciple_ObeyEarthlyLawsAndDivineJustice_368_369,
                 :EthicalPrinciple_CivicBelongingDependsOnTheGood_370_371,
                 :EthicalPrinciple_DisobedienceToRoyalLawsCondemned_380_382 .

