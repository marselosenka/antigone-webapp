############################
# CANONICAL LINES (Ancient Greek MASTER)
############################

:Line_223 a owl:NamedIndividual, :Line ;
  :lineNumber 223 ;
  :canonicalReference "223"^^xsd:string ;
  :text "Ἄναξ, ἐρῶ μὲν οὐχ ὅπως τάχους ὕπο"^^xsd:string .

:Line_224 a owl:NamedIndividual, :Line ;
  :lineNumber 224 ;
  :canonicalReference "224"^^xsd:string ;
  :text "δύσπνους ἱκάνω κοῦφον ἐξάρας πόδα."^^xsd:string .

:Line_225 a owl:NamedIndividual, :Line ;
  :lineNumber 225 ;
  :canonicalReference "225"^^xsd:string ;
  :text "Πολλὰς γὰρ ἔσχον φροντίδων ἐπιστάσεις,"^^xsd:string .

:Line_226 a owl:NamedIndividual, :Line ;
  :lineNumber 226 ;
  :canonicalReference "226"^^xsd:string ;
  :text "ὁδοῖς κυκλῶν ἐμαυτὸν εἰς ἀναστροφήν·"^^xsd:string .

:Line_227 a owl:NamedIndividual, :Line ;
  :lineNumber 227 ;
  :canonicalReference "227"^^xsd:string ;
  :text "ψυχὴ γὰρ ηὔδα πολλά μοι μυθουμένη,"^^xsd:string .

:Line_228 a owl:NamedIndividual, :Line ;
  :lineNumber 228 ;
  :canonicalReference "228"^^xsd:string ;
  :text "τάλας, τί χωρεῖς οἷ μολὼν δώσεις δίκην;"^^xsd:string .

:Line_229 a owl:NamedIndividual, :Line ;
  :lineNumber 229 ;
  :canonicalReference "229"^^xsd:string ;
  :text "τλήμων, μενεῖς αὖ; κεἰ τάδ ̓ εἴσεται Κρέων"^^xsd:string .

:Line_230 a owl:NamedIndividual, :Line ;
  :lineNumber 230 ;
  :canonicalReference "230"^^xsd:string ;
  :text "ἄλλου παρ ̓ ἀνδρός, πῶς σὺ δῆτ ̓ οὐκ ἀλγυνῇ;"^^xsd:string .

:Line_231 a owl:NamedIndividual, :Line ;
  :lineNumber 231 ;
  :canonicalReference "231"^^xsd:string ;
  :text "Τοιαῦθ ̓ ἑλίσσων ἤνυτον σχολῇ βραδύς,"^^xsd:string .

:Line_232 a owl:NamedIndividual, :Line ;
  :lineNumber 232 ;
  :canonicalReference "232"^^xsd:string ;
  :text "χοὔτως ὁδὸς βραχεῖα γίγνεται μακρά."^^xsd:string .

:Line_233 a owl:NamedIndividual, :Line ;
  :lineNumber 233 ;
  :canonicalReference "233"^^xsd:string ;
  :text "Τέλος γε μέντοι δεῦρ ̓ ἐνίκησεν μολεῖν"^^xsd:string .

:Line_234 a owl:NamedIndividual, :Line ;
  :lineNumber 234 ;
  :canonicalReference "234"^^xsd:string ;
  :text "σοί· κεἰ τὸ μηδὲν ἐξερῶ, φράσω δ ̓ ὅμως."^^xsd:string .

:Line_235 a owl:NamedIndividual, :Line ;
  :lineNumber 235 ;
  :canonicalReference "235"^^xsd:string ;
  :text "Τῆς ἐλπίδος γὰρ ἔρχομαι δεδραγμένος,"^^xsd:string .

:Line_236 a owl:NamedIndividual, :Line ;
  :lineNumber 236 ;
  :canonicalReference "236"^^xsd:string ;
  :text "τὸ μὴ παθεῖν ἂν ἄλλο πλὴν τὸ μόρσιμον."^^xsd:string .

:Line_237 a owl:NamedIndividual, :Line ;
  :lineNumber 237 ;
  :canonicalReference "237"^^xsd:string ;
  :text "Τί δ ̓ ἐστὶν ἀνθ ̓ οὗ τήνδ ̓ ἔχεις ἀθυμίαν;"^^xsd:string .

:Line_238 a owl:NamedIndividual, :Line ;
  :lineNumber 238 ;
  :canonicalReference "238"^^xsd:string ;
  :text "Φράσαι θέλω σοι πρῶτα τἀμαυτοῦ· τὸ γὰρ"^^xsd:string .

:Line_239 a owl:NamedIndividual, :Line ;
  :lineNumber 239 ;
  :canonicalReference "239"^^xsd:string ;
  :text "πρᾶγμ ̓ οὔτ ̓ ἔδρασ ̓ οὔτ ̓ εἶδον ὅστις ἦν ὁ δρῶν"^^xsd:string .

:Line_240 a owl:NamedIndividual, :Line ;
  :lineNumber 240 ;
  :canonicalReference "240"^^xsd:string ;
  :text "οὐδ ̓ ἂν δικαίως ἐς κακὸν πέσοιμί τι."^^xsd:string .

:Line_241 a owl:NamedIndividual, :Line ;
  :lineNumber 241 ;
  :canonicalReference "241"^^xsd:string ;
  :text "Εὖ γε στοχάζῃ κἀποφράγνυσαι κύκλῳ"^^xsd:string .

:Line_242 a owl:NamedIndividual, :Line ;
  :lineNumber 242 ;
  :canonicalReference "242"^^xsd:string ;
  :text "τὸ πρᾶγμα· δηλοῖς δ ̓ ὥς τι σημανῶν νέον."^^xsd:string .

:Line_243 a owl:NamedIndividual, :Line ;
  :lineNumber 243 ;
  :canonicalReference "243"^^xsd:string ;
  :text "Τὰ δεινὰ γάρ τοι προστίθησ ̓ ὄκνον πολύν."^^xsd:string .

:Line_244 a owl:NamedIndividual, :Line ;
  :lineNumber 244 ;
  :canonicalReference "244"^^xsd:string ;
  :text "Οὔκουν ἐρεῖς ποτ ̓, εἶτ ̓ ἀπαλλαχθεὶς ἄπει;"^^xsd:string .

:Line_245 a owl:NamedIndividual, :Line ;
  :lineNumber 245 ;
  :canonicalReference "245"^^xsd:string ;
  :text "Καὶ δὴ λέγω σοι· τὸν νεκρόν τις ἀρτίως"^^xsd:string .

:Line_246 a owl:NamedIndividual, :Line ;
  :lineNumber 246 ;
  :canonicalReference "246"^^xsd:string ;
  :text "θάψας βέβηκε κἀπὶ χρωτὶ διψίαν"^^xsd:string .

:Line_247 a owl:NamedIndividual, :Line ;
  :lineNumber 247 ;
  :canonicalReference "247"^^xsd:string ;
  :text "κόνιν παλύνας κἀφαγιστεύσας ἃ χρή."^^xsd:string .

:Line_248 a owl:NamedIndividual, :Line ;
  :lineNumber 248 ;
  :canonicalReference "248"^^xsd:string ;
  :text "Τί φῄς; τίς ἀνδρῶν ἦν ὁ τολμήσας τάδε;"^^xsd:string .

:Line_249 a owl:NamedIndividual, :Line ;
  :lineNumber 249 ;
  :canonicalReference "249"^^xsd:string ;
  :text "Οὐκ οἶδ ̓· ἐκεῖ γὰρ οὔτε του γενῇδος ἦν"^^xsd:string .

:Line_250 a owl:NamedIndividual, :Line ;
  :lineNumber 250 ;
  :canonicalReference "250"^^xsd:string ;
  :text "πλῆγμ ̓, οὐ δικέλλης ἐκβολή· στύφλος δὲ γῆ"^^xsd:string .

:Line_251 a owl:NamedIndividual, :Line ;
  :lineNumber 251 ;
  :canonicalReference "251"^^xsd:string ;
  :text "καὶ χέρσος, ἀρρὼξ οὐδ ̓ ἐπημαξευμένη"^^xsd:string .

:Line_252 a owl:NamedIndividual, :Line ;
  :lineNumber 252 ;
  :canonicalReference "252"^^xsd:string ;
  :text "τροχοῖσιν, ἀλλ ̓ ἄσημος οὑργάτης τις ἦν."^^xsd:string .

############################
# TRANSLATION VARIANTS (English) — ONLY TranslationVariant individuals
############################

:TV_Line_223_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_223 ; :text "Dread liege, I shall not say that I come"^^xsd:string .
:TV_Line_224_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_224 ; :text "With breathless speed, lifting up nimble feet."^^xsd:string .
:TV_Line_225_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_225 ; :text "For I stopped many a time with pangs of thought,"^^xsd:string .
:TV_Line_226_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_226 ; :text "And on the road did wheel myself around to return:"^^xsd:string .
:TV_Line_227_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_227 ; :text "For my mind was speaking much to me, saying:"^^xsd:string .
:TV_Line_228_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_228 ; :text "“Wretch, wherefore hiest thou to thy punition?"^^xsd:string .
:TV_Line_229_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_229 ; :text "Dog, tarriest thou again? An Creon should know this"^^xsd:string .
:TV_Line_230_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_230 ; :text "From some other man? Wilt thou not indeed grieve for’t?”"^^xsd:string .
:TV_Line_231_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_231 ; :text "Thus debating, I was going in leisure slowly."^^xsd:string .
:TV_Line_232_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_232 ; :text "And thus a short road became long."^^xsd:string .
:TV_Line_233_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_233 ; :text "At last, however, the day prevailed when I should come"^^xsd:string .
:TV_Line_234_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_234 ; :text "Hither to you. And if I shall say nought, I shall utter it the same:"^^xsd:string .
:TV_Line_235_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_235 ; :text "For grasping one hope I arrive,"^^xsd:string .
:TV_Line_236_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_236 ; :text "Not to suffer but that which is fated."^^xsd:string .
:TV_Line_237_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_237 ; :text "What’s got you so out of your wits?"^^xsd:string .
:TV_Line_238_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_238 ; :text "I wish first to tell you of myself: for neither"^^xsd:string .
:TV_Line_239_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_239 ; :text "Did I the deed, nor know I who hath it done."^^xsd:string .
:TV_Line_240_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_240 ; :text "And it were not right that I should suffer for it."^^xsd:string .
:TV_Line_241_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_241 ; :text "You’re taking your aim and planting defenses"^^xsd:string .
:TV_Line_242_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_242 ; :text "Around the matter; you must have something strange to say."^^xsd:string .
:TV_Line_243_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_243 ; :text "Verily, for dread matters yieldeth great hesitation."^^xsd:string .
:TV_Line_244_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_244 ; :text "Just say what you need to say and be gone."^^xsd:string .
:TV_Line_245_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_245 ; :text "Indeed, I tell you: the corpse, someone even now"^^xsd:string .
:TV_Line_246_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_246 ; :text "hath honored its last rites and departed: sprinkling"^^xsd:string .
:TV_Line_247_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_247 ; :text "thirsty dust upon the flesh in obsequies, as is enjoined."^^xsd:string .
:TV_Line_248_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_248 ; :text "What are you saying? Which man dared to do this?"^^xsd:string .
:TV_Line_249_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_249 ; :text "I wit not: for, neither are there marks of axe nor pitchfork,"^^xsd:string .

# NOTE: English line “The sodden turf sits dry and hard, neither are there the marks” corresponds to Greek lines 250–251,
# so it is attached to BOTH canonical Greek lines below.

:TV_Line_250_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_250 ; :text "The sodden turf sits dry and hard, neither are there the marks"^^xsd:string .
:TV_Line_251_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_251 ; :text "The sodden turf sits dry and hard, neither are there the marks"^^xsd:string .

:TV_Line_252_En a owl:NamedIndividual, :TranslationVariant ; :relatedTo :Line_252 ; :text "Of a wagon: no trace hath the man left behind him."^^xsd:string .

############################
# INTENTIONS AND OBJECTIVES
############################

:Motivation_Guard_SelfPreservation_1
  a owl:NamedIndividual, :Motivation ;
  :description "Avoid punishment and preserve his life while delivering bad news to Creon."^^xsd:string .

:Motivation_Guard_ReportToAuthority_1
  a owl:NamedIndividual, :Motivation ;
  :description "Report the unauthorized burial to Creon despite fear and hesitation."^^xsd:string .

:MoralDecision_Guard_ComeAndSpeak_1
  a owl:NamedIndividual, :MoralDecision ;
  :description "Decide to come before Creon and speak, even while fearing consequences."^^xsd:string .

:Motivation_Creon_IdentifyPerpetrator_1
  a owl:NamedIndividual, :Motivation ;
  :description "Discover who dared to bury the corpse and assert control over the violation."^^xsd:string .

:MoralDecision_Creon_DemandDisclosure_1
  a owl:NamedIndividual, :MoralDecision ;
  :description "Press the guard to disclose the matter and identify the offender."^^xsd:string .

:Antigone_Character_Guard
  :hasMotivation :Motivation_Guard_SelfPreservation_1, :Motivation_Guard_ReportToAuthority_1 ;
  :makesMoralDecision :MoralDecision_Guard_ComeAndSpeak_1 .

:Antigone_Character_Creon
  :hasMotivation :Motivation_Creon_IdentifyPerpetrator_1 ;
  :makesMoralDecision :MoralDecision_Creon_DemandDisclosure_1 .

############################
# MORAL JUSTIFICATIONS
############################

:EthicalPrinciple_UnjustPunishmentWrong_1
  a owl:NamedIndividual, :EthicalPrinciple ;
  :description "It is not right to suffer punishment for a deed one did not commit or witness."^^xsd:string .

:Antigone_Character_Guard
  :advocatesFor :EthicalPrinciple_UnjustPunishmentWrong_1 .

############################
# EMOTIONAL STATES
############################

:Emotion_Guard_FearAndHesitation_1
  a owl:NamedIndividual, :Emotion ;
  :description "Fearful hesitation and anxiety about punishment while approaching Creon."^^xsd:string .

:Emotion_Creon_Suspicion_1
  a owl:NamedIndividual, :Emotion ;
  :description "Suspicion that the guard brings troubling news and is shielding himself."^^xsd:string .

:Antigone_Character_Guard
  :experiencesEmotion :Emotion_Guard_FearAndHesitation_1 .

:Antigone_Character_Creon
  :experiencesEmotion :Emotion_Creon_Suspicion_1 .

############################
# SEMANTIC CONTRADICTIONS
############################

:Conflict_Guard_InnocenceVsSuspicion_1
  a owl:NamedIndividual, :Conflict ;
  :description "Guard claims innocence while Creon treats his defensiveness as a sign of troubling news."^^xsd:string ;
  :conflictBetween :Antigone_Character_Guard, :Antigone_Character_Creon .

:Conflict_Guard_Internal_1
  a owl:NamedIndividual, :Conflict ;
  :description "Guard’s self-preservation clashes with his need to report the burial to authority."^^xsd:string ;
  :conflictBetween :Motivation_Guard_SelfPreservation_1, :Motivation_Guard_ReportToAuthority_1 .

