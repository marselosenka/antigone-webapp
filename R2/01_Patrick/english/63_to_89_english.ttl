:Line_063 a :Line ; :lineNumber 63 ; :canonicalReference "63"^^xsd:string .
:TV_Line_063_en a :TranslationVariant ; :text "We are ruled by whoever’s stronger,"^^xsd:string ; :relatedTo :Line_063 .
:Line_063 :hasTheme :Theme_ObedienceToPower .

:Line_064 a :Line ; :lineNumber 64 ; :canonicalReference "64"^^xsd:string .
:TV_Line_064_en a :TranslationVariant ; :text "And as a result we must obey this order and worse."^^xsd:string ; :relatedTo :Line_064 .
:Line_064 :hasTheme :Theme_ObedienceToPower ; :relatedTo :Law_CivicOrder .

:Line_065 a :Line ; :lineNumber 65 ; :canonicalReference "65"^^xsd:string .
:TV_Line_065_en a :TranslationVariant ; :text "I am bound by these circumstances—"^^xsd:string ; :relatedTo :Line_065 .
:Line_065 :hasTheme :Theme_ObedienceToPower .

:Line_066 a :Line ; :lineNumber 66 ; :canonicalReference "66"^^xsd:string .
:TV_Line_066_en a :TranslationVariant ; :text "Even as I beg the gods of death for mercy,"^^xsd:string ; :relatedTo :Line_066 .
:Line_066 :hasTheme :Theme_LoyaltyToUnderworld ; :referencesMyth :Myth_GodsOfDeath .

:Line_067 a :Line ; :lineNumber 67 ; :canonicalReference "67"^^xsd:string .
:TV_Line_067_en a :TranslationVariant ; :text "I will obey those in power; there’s no sense"^^xsd:string ; :relatedTo :Line_067 .
:Line_067 :hasTheme :Theme_ObedienceToPower ; :relatedTo :Motivation_SubmitToPower .

:Line_068 a :Line ; :lineNumber 68 ; :canonicalReference "68"^^xsd:string .
:TV_Line_068_en a :TranslationVariant ; :text "In meddling where you don’t belong."^^xsd:string ; :relatedTo :Line_068 .
:Line_068 :hasTheme :Theme_ObedienceToPower ; :relatedTo :Motivation_AvoidMeddling .

:Line_069 a :Line ; :lineNumber 69 ; :canonicalReference "69"^^xsd:string .
:TV_Line_069_en a :TranslationVariant ; :text "I’m not going to force you, but even if you change"^^xsd:string ; :relatedTo :Line_069 .
:Line_069 :hasTheme :Theme_BurialDuty .

:Line_070 a :Line ; :lineNumber 70 ; :canonicalReference "70"^^xsd:string .
:TV_Line_070_en a :TranslationVariant ; :text "Your mind, I won’t accept your help now."^^xsd:string ; :relatedTo :Line_070 .
:Line_070 :hasTheme :Theme_BurialDuty ; :hasConflict :Conflict_SecrecyVsProclamation .

:Line_071 a :Line ; :lineNumber 71 ; :canonicalReference "71"^^xsd:string .
:TV_Line_071_en a :TranslationVariant ; :text "Do what you think is right; I will bury him."^^xsd:string ; :relatedTo :Line_071 .
:Line_071 :hasTheme :Theme_BurialDuty ; :relatedTo :Motivation_BuryBrother .

:Line_072 a :Line ; :lineNumber 72 ; :canonicalReference "72"^^xsd:string .
:TV_Line_072_en a :TranslationVariant ; :text "It’s a beautiful thing, to die in this act."^^xsd:string ; :relatedTo :Line_072 .
:Line_072 :hasTheme :Theme_DeathAsHonor ; :relatedTo :MoralDecision_BuryBrotherAndAcceptDeath .

:Line_073 a :Line ; :lineNumber 73 ; :canonicalReference "73"^^xsd:string .
:TV_Line_073_en a :TranslationVariant ; :text "With him I will rest, beloved with beloved,"^^xsd:string ; :relatedTo :Line_073 .
:Line_073 :hasTheme :Theme_LoyaltyToUnderworld ; :relatedTo :Antigone_Character_Brother .

:Line_074 a :Line ; :lineNumber 74 ; :canonicalReference "74"^^xsd:string .
:TV_Line_074_en a :TranslationVariant ; :text "And I will be a god-fearing criminal. I need to please"^^xsd:string ; :relatedTo :Line_074 .
:Line_074 :hasTheme :Theme_DivineHonorForDead ; :referencesMyth :Myth_GodsOfDeath .

:Line_075 a :Line ; :lineNumber 75 ; :canonicalReference "75"^^xsd:string .
:TV_Line_075_en a :TranslationVariant ; :text "The gods of death more than the men on Earth,"^^xsd:string ; :relatedTo :Line_075 .
:Line_075 :hasTheme :Theme_LoyaltyToUnderworld ; :referencesMyth :Myth_GodsOfDeath ; :hasConflict :Conflict_DivineHonorVsCivicOrder .

:Line_076 a :Line ; :lineNumber 76 ; :canonicalReference "76"^^xsd:string .
:TV_Line_076_en a :TranslationVariant ; :text "As I will rest below forever. But if you think it best,"^^xsd:string ; :relatedTo :Line_076 .
:Line_076 :hasTheme :Theme_LoyaltyToUnderworld .

:Line_077 a :Line ; :lineNumber 77 ; :canonicalReference "77"^^xsd:string .
:TV_Line_077_en a :TranslationVariant ; :text "Go ahead and dishonor what the gods honor."^^xsd:string ; :relatedTo :Line_077 .
:Line_077 :hasTheme :Theme_DivineHonorForDead ; :hasConflict :Conflict_DivineHonorVsCivicOrder .

:Line_078 a :Line ; :lineNumber 78 ; :canonicalReference "78"^^xsd:string .
:TV_Line_078_en a :TranslationVariant ; :text "I’m not dishonoring anyone! And even still, I’m just a woman!"^^xsd:string ; :relatedTo :Line_078 .
:Line_078 :hasTheme :Theme_GenderConstraint .

:Line_079 a :Line ; :lineNumber 79 ; :canonicalReference "79"^^xsd:string .
:TV_Line_079_en a :TranslationVariant ; :text "Only men get a say in how the city’s run, you know that."^^xsd:string ; :relatedTo :Line_079 .
:Line_079 :hasTheme :Theme_GenderConstraint ; :hasTheme :Theme_ObedienceToPower ; :hasConflict :Conflict_GenderConstraintVsAgency .

:Line_080 a :Line ; :lineNumber 80 ; :canonicalReference "80"^^xsd:string .
:TV_Line_080_en a :TranslationVariant ; :text "You can make that your excuse—"^^xsd:string ; :relatedTo :Line_080 .
:Line_080 :hasTheme :Theme_GenderConstraint .

:Line_081 a :Line ; :lineNumber 81 ; :canonicalReference "81"^^xsd:string .
:TV_Line_081_en a :TranslationVariant ; :text "I will bury my brother."^^xsd:string ; :relatedTo :Line_081 .
:Line_081 :hasTheme :Theme_BurialDuty ; :relatedTo :Motivation_BuryBrother .

:Line_082 a :Line ; :lineNumber 82 ; :canonicalReference "82"^^xsd:string .
:TV_Line_082_en a :TranslationVariant ; :text "Oh my poor sister, I’m so afraid for you!"^^xsd:string ; :relatedTo :Line_082 .
:Line_082 :hasTheme :Theme_SecrecyVsProclamation ; :relatedTo :Motivation_ProtectSister .

:Line_083 a :Line ; :lineNumber 83 ; :canonicalReference "83"^^xsd:string .
:TV_Line_083_en a :TranslationVariant ; :text "Don’t fear for me! Sort your own fate out!"^^xsd:string ; :relatedTo :Line_083 .
:Line_083 :hasTheme :Theme_DeathAsHonor .

:Line_084 a :Line ; :lineNumber 84 ; :canonicalReference "84"^^xsd:string .
:TV_Line_084_en a :TranslationVariant ; :text "You mustn’t tell anyone what you’re doing—"^^xsd:string ; :relatedTo :Line_084 .
:Line_084 :hasTheme :Theme_SecrecyVsProclamation ; :hasConflict :Conflict_SecrecyVsProclamation .

:Line_085 a :Line ; :lineNumber 85 ; :canonicalReference "85"^^xsd:string .
:TV_Line_085_en a :TranslationVariant ; :text "Keep it secret, and I will, too."^^xsd:string ; :relatedTo :Line_085 .
:Line_085 :hasTheme :Theme_SecrecyVsProclamation ; :hasConflict :Conflict_SecrecyVsProclamation .

:Line_086 a :Line ; :lineNumber 86 ; :canonicalReference "86"^^xsd:string .
:TV_Line_086_en a :TranslationVariant ; :text "Oh, on the contrary! Tell them all! If you don’t spread the word"^^xsd:string ; :relatedTo :Line_086 .
:Line_086 :hasTheme :Theme_SecrecyVsProclamation ; :hasConflict :Conflict_SecrecyVsProclamation ; :relatedTo :Motivation_PublicizeDeed .

:Line_087 a :Line ; :lineNumber 87 ; :canonicalReference "87"^^xsd:string .
:TV_Line_087_en a :TranslationVariant ; :text "To everyone, they will hate you even more for biting your tongue."^^xsd:string ; :relatedTo :Line_087 .
:Line_087 :hasTheme :Theme_SecrecyVsProclamation ; :hasConflict :Conflict_SecrecyVsProclamation .

:Line_088 a :Line ; :lineNumber 88 ; :canonicalReference "88"^^xsd:string .
:TV_Line_088_en a :TranslationVariant ; :text "Your heart is hot for chilling deeds."^^xsd:string ; :relatedTo :Line_088 .
:Line_088 :hasTheme :Theme_DeathAsHonor .

:Line_089 a :Line ; :lineNumber 89 ; :canonicalReference "89"^^xsd:string .
:TV_Line_089_en a :TranslationVariant ; :text "But I know that, to those who matter most, I’m doing the right thing."^^xsd:string ; :relatedTo :Line_089 .
:Line_089 :hasTheme :Theme_DivineHonorForDead ; :hasTheme :Theme_LoyaltyToUnderworld .

