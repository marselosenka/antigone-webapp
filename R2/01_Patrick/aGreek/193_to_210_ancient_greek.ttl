:Line_193 a owl:NamedIndividual, :Line ; :lineNumber 193 ; :canonicalReference "193"^^xsd:string ;
:text "ἀστοῖσι παίδων τῶν ἀπ ̓ Οἰδίπου πέρι·"^^xsd:string ;
:partOfScene :Antigone_Scene_CreonDecree_193_210_grc .

:Line_194 a owl:NamedIndividual, :Line ; :lineNumber 194 ; :canonicalReference "194"^^xsd:string ;
