:Line_067 a :Line ; :lineNumber 67 ; :canonicalReference "67"^^xsd:string .
:TV_Line_067_grc a :TranslationVariant ; :text "τοῖς ἐν τέλει βεβῶσι πείσομαι· τὸ γὰρ"^^xsd:string ; :relatedTo :Line_067 .
:Line_067 :hasTheme :Theme_ObedienceToAuthority ; :makesMoralDecision :MoralDecision_SubmitToRulers ; :relatedTo :Law_CivicBurialBan .

:Line_068 a :Line ; :lineNumber 68 ; :canonicalReference "68"^^xsd:string .
:TV_Line_068_grc a :TranslationVariant ; :text "περισσὰ πράσσειν οὐκ ἔχει νοῦν οὐδένα."^^xsd:string ; :relatedTo :Line_068 .
:Line_068 :hasTheme :Theme_ObedienceToAuthority ; :hasTheme :Theme_PrudenceAndModeration .

:Theme_PrudenceAndModeration
a :Theme ;
:description "Η αντίληψη ότι το να πράττει κανείς 'περισσά' δεν έχει νου."^^xsd:string .

:Line_069 a :Line ; :lineNumber 69 ; :canonicalReference "69"^^xsd:string .
:TV_Line_069_grc a :TranslationVariant ; :text "Οὔτ ̓ ἂν κελεύσαιμ ̓ οὔτ ̓ ἄν, εἰ θέλοις ἔτι"^^xsd:string ; :relatedTo :Line_069 .
:Line_069 :hasTheme :Theme_SisterlyDivision ; :hasConflict :Conflict_SisterlyDivision .

:Theme_SisterlyDivision
a :Theme ;
:description "Διάσταση ανάμεσα στις αδελφές: η Αντιγόνη απορρίπτει την όψιμη συμπαράσταση."^^xsd:string .

:Line_070 a :Line ; :lineNumber 70 ; :canonicalReference "70"^^xsd:string .
:TV_Line_070_grc a :TranslationVariant ; :text "πράσσειν, ἐμοῦ γ ̓ ἂν ἡδέως δρῴης μέτα."^^xsd:string ; :relatedTo :Line_070 .
:Line_070 :hasTheme :Theme_SisterlyDivision ; :hasConflict :Conflict_SisterlyDivision .

:Line_071 a :Line ; :lineNumber 71 ; :canonicalReference "71"^^xsd:string .
:TV_Line_071_grc a :TranslationVariant ; :text "Ἀλλ ̓ ἴσθ ̓ ὁποῖά σοι δοκεῖ, κεῖνον δ ̓ ἐγὼ"^^xsd:string ; :relatedTo :Line_071 .
:Line_071 :hasTheme :Theme_BurialDuty ; :experiencesEmotion :Emotion_Resolve ; :hasMotivation :Motivation_BuryBrotherDespiteRisk .

:Line_072 a :Line ; :lineNumber 72 ; :canonicalReference "72"^^xsd:string .
:TV_Line_072_grc a :TranslationVariant ; :text "θάψω· καλόν μοι τοῦτο ποιούσῃ θανεῖν."^^xsd:string ; :relatedTo :Line_072 .
:Line_072 :hasTheme :Theme_BurialDuty ; :hasTheme :Theme_DeathAsHonor ; :makesMoralDecision :MoralDecision_BuryBrotherAndAcceptDeath ; :experiencesEmotion :Emotion_DefiantCourage .

:Line_073 a :Line ; :lineNumber 73 ; :canonicalReference "73"^^xsd:string .
:TV_Line_073_grc a :TranslationVariant ; :text "Φίλη μετ ̓ αὐτοῦ κείσομαι, φίλου μέτα,"^^xsd:string ; :relatedTo :Line_073 .
:Line_073 :hasTheme :Theme_LoyaltyToDead ; :experiencesEmotion :Emotion_ReverentDuty .

:Line_074 a :Line ; :lineNumber 74 ; :canonicalReference "74"^^xsd:string .
:TV_Line_074_grc a :TranslationVariant ; :text "ὅσια πανουργήσασ ̓· ἐπεὶ πλείων χρόνος"^^xsd:string ; :relatedTo :Line_074 .
:Line_074 :hasTheme :Theme_DivineLaw ; :advocatesFor :EthicalPrinciple_DivineHonorForDead ; :experiencesEmotion :Emotion_ReverentDuty .

:Line_075 a :Line ; :lineNumber 75 ; :canonicalReference "75"^^xsd:string .
:TV_Line_075_grc a :TranslationVariant ; :text "ὃν δεῖ μ ̓ ἀρέσκειν τοῖς κάτω τῶν ἐνθάδε."^^xsd:string ; :relatedTo :Line_075 .
:Line_075 :hasTheme :Theme_LoyaltyToDead ; :hasMotivation :Motivation_PleaseUnderworld .

:Line_076 a :Line ; :lineNumber 76 ; :canonicalReference "76"^^xsd:string .
:TV_Line_076_grc a :TranslationVariant ; :text "Ἐκεῖ γὰρ αἰεὶ κείσομαι· σοὶ δ ̓ εἰ δοκεῖ,"^^xsd:string ; :relatedTo :Line_076 .
:Line_076 :hasTheme :Theme_LoyaltyToDead ; :experiencesEmotion :Emotion_Resolve .

:Line_077 a :Line ; :lineNumber 77 ; :canonicalReference "77"^^xsd:string .
:TV_Line_077_grc a :TranslationVariant ; :text "τὰ τῶν θεῶν ἔντιμ ̓ ἀτιμάσασ ̓ ἔχε."^^xsd:string ; :relatedTo :Line_077 .
:Line_077 :hasTheme :Theme_DivineLaw ; :hasConflict :Conflict_DivineLawVsCivicAuthority ; :advocatesFor :EthicalPrinciple_DivineHonorForDead .

:Line_078 a :Line ; :lineNumber 78 ; :canonicalReference "78"^^xsd:string .
:TV_Line_078_grc a :TranslationVariant ; :text "Ἐγὼ μὲν οὐκ ἄτιμα ποιοῦμαι, τὸ δὲ"^^xsd:string ; :relatedTo :Line_078 .
:Line_078 :hasTheme :Theme_DivineLaw ; :experiencesEmotion :Emotion_Resignation .

:Line_079 a :Line ; :lineNumber 79 ; :canonicalReference "79"^^xsd:string .
:TV_Line_079_grc a :TranslationVariant ; :text "βίᾳ πολιτῶν δρᾶν ἔφυν ἀμήχανος."^^xsd:string ; :relatedTo :Line_079 .
:Line_079 :hasTheme :Theme_ObedienceToAuthority ; :hasMotivation :Motivation_AvoidForcedAction ; :experiencesEmotion :Emotion_Resignation ; :relatedTo :Law_CivicBurialBan .

:Line_080 a :Line ; :lineNumber 80 ; :canonicalReference "80"^^xsd:string .
:TV_Line_080_grc a :TranslationVariant ; :text "Σὺ μὲν τάδ ̓ ἂν προὔχοι ̓· ἐγὼ δὲ δὴ τάφον"^^xsd:string ; :relatedTo :Line_080 .
:Line_080 :hasTheme :Theme_BurialDuty ; :hasConflict :Conflict_SisterlyDivision ; :experiencesEmotion :Emotion_Resolve .

:Line_081 a :Line ; :lineNumber 81 ; :canonicalReference "81"^^xsd:string .
:TV_Line_081_grc a :TranslationVariant ; :text "χώσουσ ̓ ἀδελφῷ φιλτάτῳ πορεύσομαι."^^xsd:string ; :relatedTo :Line_081 .
:Line_081 :hasTheme :Theme_BurialDuty ; :hasMotivation :Motivation_BuryBrotherDespiteRisk ; :makesMoralDecision :MoralDecision_BuryBrotherAndAcceptDeath ; :experiencesEmotion :Emotion_Resolve .

:Line_082 a :Line ; :lineNumber 82 ; :canonicalReference "82"^^xsd:string .
:TV_Line_082_grc a :TranslationVariant ; :text "Οἴμοι ταλαίνης ὡς ὑπερδέδοικά σου."^^xsd:string ; :relatedTo :Line_082 .
:Line_082 :experiencesEmotion :Emotion_FearForAntigone ; :hasTheme :Theme_SisterlyCare .

:Theme_SisterlyCare
a :Theme ;
:description "Η στοργή/φροντίδα της Ισμήνης για την Αντιγόνη."^^xsd:string .

:Line_083 a :Line ; :lineNumber 83 ; :canonicalReference "83"^^xsd:string .
:TV_Line_083_grc a :TranslationVariant ; :text "Μὴ  ̓μοῦ προτάρβει· τὸν σὸν ἐξόρθου πότμον."^^xsd:string ; :relatedTo :Line_083 .
:Line_083 :hasTheme :Theme_SisterlyDivision ; :experiencesEmotion :Emotion_DefiantCourage .

:Line_084 a :Line ; :lineNumber 84 ; :canonicalReference "84"^^xsd:string .
:TV_Line_084_grc a :TranslationVariant ; :text "Ἀλλ ̓ οὖν προμηνύσῃς γε τοῦτο μηδενὶ"^^xsd:string ; :relatedTo :Line_084 .
