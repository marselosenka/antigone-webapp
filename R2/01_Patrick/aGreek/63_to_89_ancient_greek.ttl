:Line_066 a :Line ; :lineNumber 66 ; :canonicalReference "66"^^xsd:string .
:TV_Line_066_grc a :TranslationVariant ; :text "ξύγγνοιαν ἴσχειν, ὡς βιάζομαι τάδε,"^^xsd:string ; :relatedTo :Line_066 .
