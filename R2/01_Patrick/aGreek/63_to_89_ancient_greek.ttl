:Line_085 a :Line ; :lineNumber 85 ; :canonicalReference "85"^^xsd:string .
:TV_Line_085_grc a :TranslationVariant ; :text "τοὔργον, κρυφῇ δὲ κεῦθε, σὺν δ ̓ αὔτως ἐγώ."^^xsd:string ; :relatedTo :Line_085 .
:Line_085 :hasTheme :Theme_SecrecyVsPublicDeclaration ; :hasMotivation :Motivation_KeepPlanSecret ; :hasConflict :Conflict_SecrecyVsWitness .

:Line_086 a :Line ; :lineNumber 86 ; :canonicalReference "86"^^xsd:string .
:TV_Line_086_grc a :TranslationVariant ; :text "Οἴμοι, καταύδα· πολλὸν ἐχθίων ἔσῃ"^^xsd:string ; :relatedTo :Line_086 .
:Line_086 :hasTheme :Theme_SecrecyVsPublicDeclaration ; :experiencesEmotion :Emotion_ContemptForSilence ; :hasMotivation :Motivation_PublicWitnessForJustice ; :hasConflict :Conflict_SecrecyVsWitness .

:Line_087 a :Line ; :lineNumber 87 ; :canonicalReference "87"^^xsd:string .
:TV_Line_087_grc a :TranslationVariant ; :text "σιγῶσ ̓, ἐὰν μὴ πᾶσι κηρύξῃς τάδε."^^xsd:string ; :relatedTo :Line_087 .
:Line_087 :hasTheme :Theme_SecrecyVsPublicDeclaration ; :hasMotivation :Motivation_PublicWitnessForJustice ; :hasConflict :Conflict_SecrecyVsWitness .

:Line_088 a :Line ; :lineNumber 88 ; :canonicalReference "88"^^xsd:string .
:TV_Line_088_grc a :TranslationVariant ; :text "Θερμὴν ἐπὶ ψυχροῖσι καρδίαν ἔχεις."^^xsd:string ; :relatedTo :Line_088 .
:Line_088 :hasTheme :Theme_SisterlyCare ; :experiencesEmotion :Emotion_WarmHeartedConcern .

:Line_089 a :Line ; :lineNumber 89 ; :canonicalReference "89"^^xsd:string .
:TV_Line_089_grc a :TranslationVariant ; :text "Ἀλλ ̓ οἶδ ̓ ἀρέσκουσ ̓ οἷς μάλισθ ̓ ἁδεῖν με χρή."^^xsd:string ; :relatedTo :Line_089 .
:Line_089 :hasTheme :Theme_LoyaltyToDead ; :hasTheme :Theme_DivineLaw ; :hasMotivation :Motivation_PleaseUnderworld ; :experiencesEmotion :Emotion_ReverentDuty .
