:Line_063 a :Line ; :lineNumber 63 ; :canonicalReference "63"^^xsd:string .
:TV_Line_063_grc a :TranslationVariant ; :text "ἔπειτα δ ̓ οὕνεκ ̓ ἀρχόμεσθ ̓ ἐκ κρεισσόνων"^^xsd:string ; :relatedTo :Line_063 .
:Line_063 :hasTheme :Theme_ObedienceToAuthority .

:Line_064 a :Line ; :lineNumber 64 ; :canonicalReference "64"^^xsd:string .
:TV_Line_064_grc a :TranslationVariant ; :text "καὶ ταῦτ ̓ ἀκούειν κἄτι τῶνδ ̓ ἀλγίονα."^^xsd:string ; :relatedTo :Line_064 .
:Line_064 :hasTheme :Theme_ObedienceToAuthority ; :experiencesEmotion :Emotion_Resignation .

:Line_065 a :Line ; :lineNumber 65 ; :canonicalReference "65"^^xsd:string .
:TV_Line_065_grc a :TranslationVariant ; :text "Ἐγὼ μὲν οὖν αἰτοῦσα τοὺς ὑπὸ χθονὸς"^^xsd:string ; :relatedTo :Line_065 .
