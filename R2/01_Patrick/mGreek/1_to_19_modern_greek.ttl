:Myth_Zeus
a :MythologicalBackground ;
:description "Επίκληση στον Δία ως θεϊκή δύναμη που ζητεί πληρωμή/δικαίωση και μπορεί (ή όχι) να λυπηθεί."^^xsd:string .

:Myth_OedipusTroubles
a :MythologicalBackground ;
:description "Η αμαρτία και τα δεινά του Οιδίποδα ως πηγή της δυστυχίας των παιδιών του."^^xsd:string .

:Event_BrothersMutualDeprivation
a owl:Thing ;
:description "Τα αδέλφια στέρησαν ο ένας τον άλλον, σαν εχθροί."^^xsd:string .

:Consequence_SistersLeftAmongEnemies
a owl:Thing ;
:description "Οι αδελφές μένουν ανάμεσα σε εχθρούς, χωρίς στήριγμα."^^xsd:string .

:Event_BrothersMutualDeprivation
:relatedTo :Consequence_SistersLeftAmongEnemies .

:Line_001 a :Line ; :lineNumber 1 ; :canonicalReference "1"^^xsd:string .
:TV_Line_001_el a :TranslationVariant ; :text "Έλα καλή μου Ισμήνη. Έλα, και πες μου ειλικρινά,"^^xsd:string ; :relatedTo :Line_001 .

:Line_002 a :Line ; :lineNumber 2 ; :canonicalReference "2"^^xsd:string .
:TV_Line_002_el a :TranslationVariant ; :text "σαν αδελφή προς αδελφή, αν ξέρεις αμαρτία"^^xsd:string ; :relatedTo :Line_002 .

:Line_003 a :Line ; :lineNumber 3 ; :canonicalReference "3"^^xsd:string .
:TV_Line_003_el a :TranslationVariant ; :text "του Οιδίποδα που ο Δίας να μην ζήτησε να πληρωθεί από μας -"^^xsd:string ; :relatedTo :Line_003 .

:Line_004 a :Line ; :lineNumber 4 ; :canonicalReference "4"^^xsd:string .
:TV_Line_004_el a :TranslationVariant ; :text "μόνο και μόνο επειδή ατυχήσαμε να ζούμε."^^xsd:string ; :relatedTo :Line_004 .

:Line_005 a :Line ; :lineNumber 5 ; :canonicalReference "5"^^xsd:string .
:TV_Line_005_el a :TranslationVariant ; :text "Γιατί, εγώ τουλάχιστον, δεν βλέπω πόνο, συντριβή,"^^xsd:string ; :relatedTo :Line_005 .

:Line_006 a :Line ; :lineNumber 6 ; :canonicalReference "6"^^xsd:string .
:TV_Line_006_el a :TranslationVariant ; :text "αίσχος και ατιμία, που να μην καταβάλαμε."^^xsd:string ; :relatedTo :Line_006 .

:Line_007 a :Line ; :lineNumber 7 ; :canonicalReference "7"^^xsd:string .
:TV_Line_007_el a :TranslationVariant ; :text "Σήμερα πάλι, κυκλοφορεί αδέσποτη στην πόλη"^^xsd:string ; :relatedTo :Line_007 .

:Line_008 a :Line ; :lineNumber 8 ; :canonicalReference "8"^^xsd:string .
:TV_Line_008_el a :TranslationVariant ; :text "καινούρια διαταγή του στρατηγού."^^xsd:string ; :relatedTo :Line_008 .

:Line_009 a :Line ; :lineNumber 9 ; :canonicalReference "9"^^xsd:string .
:TV_Line_009_el a :TranslationVariant ; :text "Άκουσες τίποτα ή νομίζεις πως οι εχθροί μας"^^xsd:string ; :relatedTo :Line_009 .

:Line_010 a :Line ; :lineNumber 10 ; :canonicalReference "10"^^xsd:string .
:TV_Line_010_el a :TranslationVariant ; :text "έπαψαν να μας κυνηγούν;"^^xsd:string ; :relatedTo :Line_010 .

:Line_011 a :Line ; :lineNumber 11 ; :canonicalReference "11"^^xsd:string .
:TV_Line_011_el a :TranslationVariant ; :text "Όχι, Αντιγόνη. Δεν άκουσα, μα ούτε περνά απ’ τον νου μου"^^xsd:string ; :relatedTo :Line_011 .

:Line_012 a :Line ; :lineNumber 12 ; :canonicalReference "12"^^xsd:string .
:TV_Line_012_el a :TranslationVariant ; :text "τι περισσότερο μπορούν να μας κάνουν οι εχθροί μας·"^^xsd:string ; :relatedTo :Line_012 .

:Line_013 a :Line ; :lineNumber 13 ; :canonicalReference "13"^^xsd:string .
:TV_Line_013_el a :TranslationVariant ; :text "αφού τα αδέλφια μας, οι μόνοι που μας είχαν απομείνει,"^^xsd:string ; :relatedTo :Line_013 .

:Line_014 a :Line ; :lineNumber 14 ; :canonicalReference "14"^^xsd:string .
:TV_Line_014_el a :TranslationVariant ; :text "φρόντισαν να μας στερήσουν ο ένας τον άλλον σαν εχθροί"^^xsd:string ; :relatedTo :Line_014 .

:Line_015 a :Line ; :lineNumber 15 ; :canonicalReference "15"^^xsd:string .
:TV_Line_015_el a :TranslationVariant ; :text "και οι εχθροί Αργείοι έφυγαν και μας άφησαν"^^xsd:string ; :relatedTo :Line_015 .

:Line_016 a :Line ; :lineNumber 16 ; :canonicalReference "16"^^xsd:string .
:TV_Line_016_el a :TranslationVariant ; :text "ανάμεσα σ’ εχθρούς... χειρότερα ή καλύτερα,"^^xsd:string ; :relatedTo :Line_016 .

:Line_017 a :Line ; :lineNumber 17 ; :canonicalReference "17"^^xsd:string .
:TV_Line_017_el a :TranslationVariant ; :text "πραγματικά δεν ξέρω."^^xsd:string ; :relatedTo :Line_017 .

:Line_018 a :Line ; :lineNumber 18 ; :canonicalReference "18"^^xsd:string .
:TV_Line_018_el a :TranslationVariant ; :text "Το φαντάστηκα. Γι’ αυτό θέλησα να βρεθούμε"^^xsd:string ; :relatedTo :Line_018 .

:Line_019 a :Line ; :lineNumber 19 ; :canonicalReference "19"^^xsd:string .
:TV_Line_019_el a :TranslationVariant ; :text "μόνες εδώ, έξω από το προαύλιο."^^xsd:string ; :relatedTo :Line_019 .
