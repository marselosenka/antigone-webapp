    rdfs:domain [ a owl:Class ; owl:unionOf ( :Annotation :CriticalInterpretation ) ] ;
    rdfs:range xsd:float ;
    rdfs:comment "Confidence in the validity of an interpretative or annotational assertion." .

################################################################
# Example property characteristics (original block preserved)
################################################################

:appearsIn a owl:ObjectProperty ;
    rdfs:label "appearsIn" ;
    rdfs:domain :Character ;
    rdfs:range :Scene ;
    rdfs:subPropertyOf :characterAppearsIn ;
    owl:inverseOf :hasCharacter .

:advocatesFor a owl:ObjectProperty ;
    rdfs:label "advocatesFor" ;
    rdfs:domain :Character ;
    rdfs:range :EthicalPrinciple ;
    owl:inverseOf :isAdvocatedBy .

:isAdvocatedBy a owl:ObjectProperty ;
    rdfs:label "isAdvocatedBy" ;
    rdfs:domain :EthicalPrinciple ;
    rdfs:range :Character .

:determinesOutcomeOf a owl:ObjectProperty ;
    rdfs:label "determinesOutcomeOf" ;
    rdfs:domain :FateConcept ;
    rdfs:range :Conflict .

################################################################
# Simple class axioms (examples of subclassing / unions)
################################################################

:ChoralSong a owl:Class ;
    rdfs:label "ChoralSong" ;
    rdfs:subClassOf :Speech ;
    rdfs:comment "A choral speech (stasimon)." .

:Prologue a owl:Class ;
    rdfs:label "Prologue" ;
    rdfs:subClassOf :Scene .

:Exodus a owl:Class ;
    rdfs:label "Exodus" ;
    rdfs:subClassOf :Scene .

################################################################
# Few useful annotations to guide users (original)
################################################################

[] a rdfs:Comment ;
    rdfs:comment "Use :relatedTo to link concepts not covered by explicit properties. Add individuals for specific plays, scenes, characters, speeches, performances, translations." .
