@prefix :      <http://example.org/antigone#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix dc:    <http://purl.org/dc/elements/1.1/> .
@prefix crm:   <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix frbroo:<http://iflastandards.info/ns/fr/frbr/frbroo/> .

################################################################
# Ontology header
################################################################

:AntigoneOntology a owl:Ontology ;
    dc:title       "Antigone Analysis Ontology" ;
    dc:creator     "Generated by GPΤ and extended by humans" ;
    dc:description "Comprehensive ontology for multi-aspect analysis of Sophocles' Antigone" ;
    dc:date        "2025-11-16"^^xsd:date ;
    rdfs:comment   "Enriched with modelling patterns inspired by CIDOC CRM and FRBRoo, preserving the original structure." .

################################################################
# Core FRBR-like structural classes (added)
################################################################

:Work a owl:Class ;
    rdfs:label "Work" ;
    rdfs:comment "Abstract intellectual or artistic creation (e.g., Antigone as a conceptual work)." ;
    rdfs:subClassOf frbroo:F1_Work .

:TextualExpression a owl:Class ;
    rdfs:label "TextualExpression" ;
    rdfs:comment "A specific textual realization of a Work (original Greek text, a translation, etc.)." ;
    rdfs:subClassOf frbroo:F22_Self_Contained_Expression .

:Edition a owl:Class ;
    rdfs:label "Edition" ;
    rdfs:comment "A particular edition or publication expression of a TextualExpression." ;
    rdfs:subClassOf frbroo:F24_Publication_Expression .

:PhysicalCopy a owl:Class ;
    rdfs:label "PhysicalCopy" ;
    rdfs:comment "An individual physical or digital copy (Item) of an Edition." ;
    rdfs:subClassOf frbroo:F5_Item .

################################################################
# Classes (original + enriched with alignment)
################################################################

:Play a owl:Class ;
    rdfs:label "Play" ;
    rdfs:comment "A dramatic work as a whole." ;
    rdfs:subClassOf :Work , frbroo:F1_Work .

:Scene a owl:Class ;
    rdfs:label "Scene" ;
    rdfs:comment "A distinct section of the play." ;
    rdfs:subClassOf frbroo:F23_Expression_Fragment .

:Character a owl:Class ;
    rdfs:label "Character" ;
    rdfs:comment "A person or personified entity in the play." ;
    rdfs:subClassOf crm:E21_Person .

:Speech a owl:Class ;
    rdfs:label "Speech" ;
    rdfs:comment "A coherent spoken part by a character." ;
    rdfs:subClassOf frbroo:F23_Expression_Fragment .

:Line a owl:Class ;
    rdfs:label "Line" ;
    rdfs:comment "A single verse or line of text." ;
    rdfs:subClassOf frbroo:F23_Expression_Fragment .

:Theme a owl:Class ;
    rdfs:label "Theme" ;
    rdfs:comment "Abstract idea developed in the play." ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Motif a owl:Class ;
    rdfs:label "Motif" ;
    rdfs:comment "Recurring symbolic element." ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Symbol a owl:Class ;
    rdfs:label "Symbol" ;
    rdfs:comment "Object or act representing an abstract idea." ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Conflict a owl:Class ;
    rdfs:label "Conflict" ;
    rdfs:comment "Central opposition in the play." ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Foreshadowing a owl:Class ;
    rdfs:label "Foreshadowing" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Irony a owl:Class ;
    rdfs:label "Irony" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:EthicalPrinciple a owl:Class ;
    rdfs:label "EthicalPrinciple" ;
    rdfs:comment "A moral law or value in conflict (divine, civic, familial)." ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:PhilosophicalConcept a owl:Class ;
    rdfs:label "PhilosophicalConcept" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Law a owl:Class ;
    rdfs:label "Law" ;
    rdfs:comment "Divine or human regulation referenced in the play." ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:FateConcept a owl:Class ;
    rdfs:label "FateConcept" ;
    rdfs:comment "Concepts of destiny or inevitability." ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:PsychologicalTrait a owl:Class ;
    rdfs:label "PsychologicalTrait" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Emotion a owl:Class ;
    rdfs:label "Emotion" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Motivation a owl:Class ;
    rdfs:label "Motivation" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:MoralDecision a owl:Class ;
    rdfs:label "MoralDecision" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:HistoricalContext a owl:Class ;
    rdfs:label "HistoricalContext" ;
    rdfs:subClassOf crm:E4_Period .

:MythologicalBackground a owl:Class ;
    rdfs:label "MythologicalBackground" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:AthenianTragedyConvention a owl:Class ;
    rdfs:label "AthenianTragedyConvention" ;
    rdfs:comment "Dramatic conventions (chorus role, costume, staging, music)." ;
    rdfs:subClassOf crm:E55_Type .

:Performance a owl:Class ;
    rdfs:label "Performance" ;
    rdfs:comment "A staging of the play." ;
    rdfs:subClassOf frbroo:F31_Performance , crm:E7_Activity .

:Actor a owl:Class ;
    rdfs:label "Actor" ;
    rdfs:subClassOf crm:E21_Person .

:AudienceResponse a owl:Class ;
    rdfs:label "AudienceResponse" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Adaptation a owl:Class ;
    rdfs:label "Adaptation" ;
    rdfs:comment "Rewriting or reinterpreting the play in another medium." ;
    rdfs:subClassOf :Work , frbroo:F1_Work .

:RhetoricalDevice a owl:Class ;
    rdfs:label "RhetoricalDevice" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:Diction a owl:Class ;
    rdfs:label "Diction" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:SyntaxPattern a owl:Class ;
    rdfs:label "SyntaxPattern" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:MetaphorSimile a owl:Class ;
    rdfs:label "MetaphorSimile" ;
    rdfs:subClassOf :RhetoricalDevice .

:LexicalItem a owl:Class ;
    rdfs:label "LexicalItem" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:IntertextualReference a owl:Class ;
    rdfs:label "IntertextualReference" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:ComparativeWork a owl:Class ;
    rdfs:label "ComparativeWork" ;
    rdfs:subClassOf :Work .

:CriticalInterpretation a owl:Class ;
    rdfs:label "CriticalInterpretation" ;
    rdfs:subClassOf crm:E28_Conceptual_Object .

:AnalyticalMethod a owl:Class ;
    rdfs:label "AnalyticalMethod" ;
    rdfs:comment "Methodology e.g. structuralism, psychoanalysis, feminism." ;
    rdfs:subClassOf crm:E55_Type .

:Annotation a owl:Class ;
    rdfs:label "Annotation" ;
    rdfs:subClassOf crm:E13_Attribute_Assignment .

:TranslationVariant a owl:Class ;
    rdfs:label "TranslationVariant" ;
    rdfs:subClassOf :TextualExpression , frbroo:F22_Self_Contained_Expression .

# New classes for roles, stage directions, place (added)

:Role a owl:Class ;
    rdfs:label "Role" ;
    rdfs:comment "Dramatic role in a performance, representing a Character as embodied by an Actor." ;
    rdfs:subClassOf crm:E55_Type .

:StageDirection a owl:Class ;
    rdfs:label "StageDirection" ;
    rdfs:comment "Instruction for movement, gesture, sound, or other scenic action." ;
    rdfs:subClassOf crm:E33_Linguistic_Object .

:Entrance a owl:Class ;
    rdfs:label "Entrance" ;
    rdfs:subClassOf :StageDirection .

:Exit a owl:Class ;
    rdfs:label "Exit" ;
    rdfs:subClassOf :StageDirection .

:ChorusMovement a owl:Class ;
    rdfs:label "ChorusMovement" ;
    rdfs:subClassOf :StageDirection .

:SoundCue a owl:Class ;
    rdfs:label "SoundCue" ;
    rdfs:subClassOf :StageDirection .

:LightingCue a owl:Class ;
    rdfs:label "LightingCue" ;
    rdfs:subClassOf :StageDirection .

:Place a owl:Class ;
    rdfs:label "Place" ;
    rdfs:comment "Geographical place (e.g., theatre venue)." ;
    rdfs:subClassOf crm:E53_Place .

################################################################
# Object Properties (relations) – original + added
################################################################

:hasScene a owl:ObjectProperty ;
    rdfs:label "hasScene" ;
    rdfs:domain :Play ;
    rdfs:range :Scene ;
    rdfs:comment "Connects a Play to its Scenes." .

:sceneOf a owl:ObjectProperty ;
    rdfs:label "sceneOf" ;
    rdfs:domain :Scene ;
    rdfs:range :Play ;
    rdfs:inverseOf :hasScene .

:hasCharacter a owl:ObjectProperty ;
    rdfs:label "hasCharacter" ;
    rdfs:domain :Play ;
    rdfs:range :Character .

:characterAppearsIn a owl:ObjectProperty ;
    rdfs:label "characterAppearsIn" ;
    rdfs:domain :Character ;
    rdfs:range :Scene ;
    rdfs:comment "Character appears in a Scene (entrance/exit may be captured in annotation)." .

:hasSpeech a owl:ObjectProperty ;
    rdfs:label "hasSpeech" ;
    rdfs:domain :Scene ;
    rdfs:range :Speech .

:speechBy a owl:ObjectProperty ;
    rdfs:label "speechBy" ;
    rdfs:domain :Speech ;
    rdfs:range :Character .

:containsLine a owl:ObjectProperty ;
    rdfs:label "containsLine" ;
    rdfs:domain :Speech ;
    rdfs:range :Line .

:hasTheme a owl:ObjectProperty ;
    rdfs:label "hasTheme" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Play :Scene :Speech :Line :Performance :Adaptation ) ] ;
    rdfs:range :Theme ;
    rdfs:comment "Associate Themes with various containers." .

:hasMotif a owl:ObjectProperty ;
    rdfs:label "hasMotif" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Play :Scene :Line :Speech ) ] ;
    rdfs:range :Motif .

:hasSymbol a owl:ObjectProperty ;
    rdfs:label "hasSymbol" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Scene :Line :Speech :Play ) ] ;
    rdfs:range :Symbol .

:hasConflict a owl:ObjectProperty ;
    rdfs:label "hasConflict" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Play :Scene ) ] ;
    rdfs:range :Conflict .

:conflictBetween a owl:ObjectProperty ;
    rdfs:label "conflictBetween" ;
    rdfs:domain :Conflict ;
    rdfs:range [ a owl:Class ; owl:unionOf ( :Character :EthicalPrinciple :Law :FateConcept ) ] .

:advocatesFor a owl:ObjectProperty ;
    rdfs:label "advocatesFor" ;
    rdfs:domain :Character ;
    rdfs:range :EthicalPrinciple .

:violatesLaw a owl:ObjectProperty ;
    rdfs:label "violatesLaw" ;
    rdfs:domain :Character ;
    rdfs:range :Law .

:experiencesEmotion a owl:ObjectProperty ;
    rdfs:label "experiencesEmotion" ;
    rdfs:domain :Character ;
    rdfs:range :Emotion .

:hasMotivation a owl:ObjectProperty ;
    rdfs:label "hasMotivation" ;
    rdfs:domain :Character ;
    rdfs:range :Motivation .

:makesMoralDecision a owl:ObjectProperty ;
    rdfs:label "makesMoralDecision" ;
    rdfs:domain :Character ;
    rdfs:range :MoralDecision .

:developsTheme a owl:ObjectProperty ;
    rdfs:label "developsTheme" ;
    rdfs:domain :Scene ;
    rdfs:range :Theme ;
    rdfs:subPropertyOf :hasTheme .

:containsRhetoricalDevice a owl:ObjectProperty ;
    rdfs:label "containsRhetoricalDevice" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Speech :Line ) ] ;
    rdfs:range :RhetoricalDevice .

:interprets a owl:ObjectProperty ;
    rdfs:label "interprets" ;
    rdfs:domain :Performance ;
    rdfs:range :Play ;
    rdfs:subPropertyOf frbroo:R25_performed .

:reinterprets a owl:ObjectProperty ;
    rdfs:label "reinterprets" ;
    rdfs:domain :Adaptation ;
    rdfs:range :Play .

:hasActor a owl:ObjectProperty ;
    rdfs:label "hasActor" ;
    rdfs:domain :Performance ;
    rdfs:range :Actor .

:elicitsResponse a owl:ObjectProperty ;
    rdfs:label "elicitsResponse" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Performance :Scene :Play :Speech ) ] ;
    rdfs:range :AudienceResponse .

:hasHistoricalContext a owl:ObjectProperty ;
    rdfs:label "hasHistoricalContext" ;
    rdfs:domain :Play ;
    rdfs:range :HistoricalContext .

:referencesMyth a owl:ObjectProperty ;
    rdfs:label "referencesMyth" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Play :Scene :Line :Speech ) ] ;
    rdfs:range :MythologicalBackground .

:hasConvention a owl:ObjectProperty ;
    rdfs:label "hasConvention" ;
    rdfs:domain :Play ;
    rdfs:range :AthenianTragedyConvention .

:hasIntertextualReference a owl:ObjectProperty ;
    rdfs:label "hasIntertextualReference" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Line :Speech :Scene :Play ) ] ;
    rdfs:range :IntertextualReference .

:comparesWith a owl:ObjectProperty ;
    rdfs:label "comparesWith" ;
    rdfs:domain :ComparativeWork ;
    rdfs:range :Play .

:hasCriticalInterpretation a owl:ObjectProperty ;
    rdfs:label "hasCriticalInterpretation" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Play :Scene :Character :Performance :Adaptation ) ] ;
    rdfs:range :CriticalInterpretation .

:usesAnalyticalMethod a owl:ObjectProperty ;
    rdfs:label "usesAnalyticalMethod" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :CriticalInterpretation :Annotation :ComparativeWork ) ] ;
    rdfs:range :AnalyticalMethod .

:annotates a owl:ObjectProperty ;
    rdfs:label "annotates" ;
    rdfs:domain :Annotation ;
    rdfs:range [ a owl:Class ; owl:unionOf ( :Play :Scene :Line :Speech ) ] .

:hasTranslationVariant a owl:ObjectProperty ;
    rdfs:label "hasTranslationVariant" ;
    rdfs:domain :Play ;
    rdfs:range :TranslationVariant .

:relatedTo a owl:ObjectProperty ;
    rdfs:label "relatedTo" ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    rdfs:comment "Generic relatedness relation for linking concepts not captured by other properties." .

# New FRBR-like object properties (added)

:hasExpression a owl:ObjectProperty ;
    rdfs:label "hasExpression" ;
    rdfs:domain :Work ;
    rdfs:range :TextualExpression ;
    rdfs:subPropertyOf frbroo:R3_is_realised_in .

:hasEdition a owl:ObjectProperty ;
    rdfs:label "hasEdition" ;
    rdfs:domain :TextualExpression ;
    rdfs:range :Edition ;
    rdfs:subPropertyOf frbroo:R24_created .

:hasCopy a owl:ObjectProperty ;
    rdfs:label "hasCopy" ;
    rdfs:domain :Edition ;
    rdfs:range :PhysicalCopy ;
    rdfs:subPropertyOf frbroo:R29_reproduced .

# New role-related properties (added)

:representsCharacter a owl:ObjectProperty ;
    rdfs:label "representsCharacter" ;
    rdfs:domain :Role ;
    rdfs:range :Character .

:performedBy a owl:ObjectProperty ;
    rdfs:label "performedBy" ;
    rdfs:domain :Role ;
    rdfs:range :Actor ;
    rdfs:subPropertyOf crm:P14_carried_out_by .

:roleInPerformance a owl:ObjectProperty ;
    rdfs:label "roleInPerformance" ;
    rdfs:domain :Role ;
    rdfs:range :Performance .

# Stage direction / place / performance detail properties (added)

:hasStageDirection a owl:ObjectProperty ;
    rdfs:label "hasStageDirection" ;
    rdfs:domain :Scene ;
    rdfs:range :StageDirection .

:takesPlaceAt a owl:ObjectProperty ;
    rdfs:label "takesPlaceAt" ;
    rdfs:domain :Performance ;
    rdfs:range :Place ;
    rdfs:subPropertyOf crm:P7_took_place_at .

# Character relationship properties (added)

:isSiblingOf a owl:ObjectProperty , owl:SymmetricProperty ;
    rdfs:label "isSiblingOf" ;
    rdfs:domain :Character ;
    rdfs:range :Character .

:isParentOf a owl:ObjectProperty ;
    rdfs:label "isParentOf" ;
    rdfs:domain :Character ;
    rdfs:range :Character .

:isChildOf a owl:ObjectProperty ;
    rdfs:label "isChildOf" ;
    rdfs:domain :Character ;
    rdfs:range :Character ;
    owl:inverseOf :isParentOf .

:isBetrothedTo a owl:ObjectProperty , owl:SymmetricProperty ;
    rdfs:label "isBetrothedTo" ;
    rdfs:domain :Character ;
    rdfs:range :Character .

:isEnemyOf a owl:ObjectProperty , owl:SymmetricProperty ;
    rdfs:label "isEnemyOf" ;
    rdfs:domain :Character ;
    rdfs:range :Character .

:isAllyOf a owl:ObjectProperty , owl:SymmetricProperty ;
    rdfs:label "isAllyOf" ;
    rdfs:domain :Character ;
    rdfs:range :Character .

# Provenance / scholarly assertion properties (added)

:assertedBy a owl:ObjectProperty ;
    rdfs:label "assertedBy" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Annotation :CriticalInterpretation ) ] ;
    rdfs:range crm:E21_Person .

:assertedIn a owl:ObjectProperty ;
    rdfs:label "assertedIn" ;
    rdfs:domain [ a owl:Class ; owl:unionOf ( :Annotation :CriticalInterpretation ) ] ;
    rdfs:range crm:E31_Document .

